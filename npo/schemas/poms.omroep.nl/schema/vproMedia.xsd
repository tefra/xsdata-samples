<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
           targetNamespace="urn:vpro:media:2009" xmlns="urn:vpro:media:2009"
           xmlns:shared="urn:vpro:shared:2009"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
>

  <xs:import namespace="urn:vpro:shared:2009" schemaLocation="vproShared.xsd"/>

  <xs:import
    namespace="http://www.w3.org/XML/1998/namespace"
    schemaLocation="../../xml.xsd" />

  <xs:annotation>
    <xs:documentation>This is the XSD for the POMS domain.</xs:documentation>
  </xs:annotation>

  <xs:element name="mediaInformation" type="mediaTableType">
    <xs:annotation>
      <xs:documentation>
        Base element only used when programs, groups and schedule information need to be bundled in one XML. E.g. when distributing to cable companies.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="program" type="programType">
    <xs:annotation>
      <xs:documentation>
        This is the most used entity in POMS. It represents e.g. one broadcast program or one web-only clip. It represent a standalone entity which a consumer can view or listen to.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="group" type="groupType">
    <xs:annotation>
      <xs:documentation>
        A groups collects a number of programs and/or other groups. Examples: season, series, playlist and album.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="segment" type="segmentType">
    <xs:annotation>
      <xs:documentation>
        A program can contain a number of segments. A segment is an identifiable part of a program.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="schedule" type="scheduleType">
    <xs:annotation>
      <xs:documentation>
        Programs of type 'BROADCAST' can contain schedule events. A schedule indicates on which channel and at what time the program is broadcast. A schedule is a container which contains the schedule events of different programs, for a certain period of time.
      </xs:documentation>
    </xs:annotation>
  </xs:element>

    <xs:element name="streamingStatus" type="streamingStatus"/>


  <xs:complexType name="mediaTableType">
    <xs:sequence>
      <xs:element type="programTableType" name="programTable" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A table with all program objects in this container</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="groupTableType" name="groupTable" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A table with all group objects in this container</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="locationTableType" name="locationTable" minOccurs="0"/>
      <xs:element ref="schedule" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A table with all schedule information in this container</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute type="xs:string" name="publisher"/>
    <xs:attribute type="xs:dateTime" name="publicationTime"/>
    <xs:attribute type="xs:short" name="version"/>
  </xs:complexType>

  <xs:complexType name="programTableType">
    <xs:sequence>
      <xs:element ref="program" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="programType">
    <xs:complexContent>
      <xs:extension base="baseMediaType">
        <xs:sequence>
          <xs:element type="scheduleEventsType" name="scheduleEvents" minOccurs="0"/>
          <xs:element name="episodeOf" type="memberRefType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>
                A program (only if its type is 'BROADCAST') can be an episode of a group of type 'SERIES' or 'SEASON'.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="segments" type="segmentsType" minOccurs="0"/>
          <!--MSE-2330
          <xs:element name="poSeriesID" type="xs:string" minOccurs="0"/>
          <xs:element name="poProgType" type="xs:string" minOccurs="0"/>
          <xs:element name="poMiscInfo" type="xs:string" minOccurs="0"/>
          <xs:element name="poSeasonSubTitle" type="xs:string" minOccurs="0"/>
          -->
        </xs:sequence>
        <xs:attribute type="programTypeEnum" name="type" use="required">
          <xs:annotation>
            <xs:documentation>
              The type of this program (e.g. BROADCAST, TRACK, CLIP)
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

