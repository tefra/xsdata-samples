          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SEGMENTTYPE">
        <xs:annotation>
          <xs:documentation>
          The abstract type denoting every possible segment type
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="STRAND"/>
      <xs:enumeration value="ALBUM"/>
      <xs:enumeration value="PLAYLIST"/>
      <xs:enumeration value="ARCHIVE"/>
      <!--
      <xs:enumeration value="UG ARCHIVE"/>
      -->
      <xs:enumeration value="SEASON"/>
      <xs:enumeration value="SERIES"/>
      <xs:enumeration value="UMBRELLA"/>
      <xs:enumeration value="BROADCAST"/>
      <xs:enumeration value="MOVIE"/>
      <xs:enumeration value="TRAILER"/>
      <xs:enumeration value="CLIP"/>
      <xs:enumeration value="TRACK"/>
      <xs:enumeration value="SEGMENT"/>
      <xs:enumeration value="VISUALRADIO"/>
      <xs:enumeration value="VISUALRADIOSEGMENT"/>

      <xs:enumeration value="PROMO"/>
      <xs:enumeration value="RECORDING"/>
      <xs:enumeration value="COLLECTION"/>
    </xs:restriction>
