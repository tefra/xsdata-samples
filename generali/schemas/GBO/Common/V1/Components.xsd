	<xsd:complexType name="CheckType">
		<xsd:annotation>
			<xsd:documentation>The schema containing the declarations for the framework check object. This is used to support the verification of a business object with a authoritative third-party reference, e.g. Dun and Bradstreet or a Postal Address lookup.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element
						name="MadeAgainst"
						type="tns:ChecksMadeAgainstType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The object against which the set of checks have been made.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element
						name="MadeBy"
						type="tns:ChecksMadeByType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The person or organisation that made the checks against the business object.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ChecksMadeAgainstType">
		<xsd:annotation>
			<xsd:documentation>The object against which the set of checks have been made.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:BaseIdentifiedComponentType" />
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ChecksMadeByType">
		<xsd:annotation>
			<xsd:documentation>The person or organisation that made the checks against the business object.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:BaseIdentifiedComponentType" />
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="QueryType">
		<xsd:annotation>
			<xsd:documentation>
				<description>A data object that allows simple and complex query criteria to be specified against which a set of business objects can be matched.</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice maxOccurs="1">
				<xsd:element
					name="Identification"
					type="tns:BaseIdentifiedComponentType"
					maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
							<description>A component within the query that specifies a specific instance of a business to retrieve. The ID specified represents the primary key of the business object as configured through the IDs/ID[/@schemeName=''] field.</description>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence>
					<xsd:element
						name="Criteria"
						type="tns:QueryCriteriaType"
						maxOccurs="1" />
					<xsd:element
						name="SortExpression"
						type="tns:SortExpressionType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A component holding the specification of the sorting required on the query.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute
			name="businessObject"
			type="xsd:token"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>
					<description>If the Query object is being used as a generic query capability then this attribute indicates the VBO being searched on. If the query is on a specific VBS then it should not be present. If present and different from the VBO then it must be ignored.</description>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="QueryCriteriaType">
		<xsd:sequence>
			<xsd:element name="QueryExpression">
				<xsd:annotation>
					<xsd:documentation>
						<description>A query expression that allows complex queries to be formulated out of a series of value expressions (criteria).</description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:choice>
						<xsd:element
							name="ValueExpression"
							type="tns:ValueExpressionType"
							minOccurs="0"
							maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<description>The value to be matched in a query criteria. The repeating structure allows more than one value to be specified, these must be taken as an implict OR function, i.e. This Value-1 OR Value-2 OR Value-3.</description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element
							name="QueryExpression"
							type="tns:QueryExpressionType"
							minOccurs="0"
							maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>
									<description>A query expression that allows complex queries to be formulated out of a series of value expressions (criteria).</description>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute
			name="startIndex"
			type="xsd:integer"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>
					<description>A field that supports the specification of pagination by a requester. The start index indicates the number of the instance matching the query criteria from which the page to be returned starts.</description>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute
			name="indexPerPage"
			type="xsd:integer"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>
					<description>A field that supports the specification of the size of the page to be returned in a query.</description>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute
			name="matchingAlgorithm"
			type="tns:QueryAlgorithmCodeType"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>
					<description>An optional indicator of either the strength of matching or the specific algorithm to use in matching the records on the server. The default is Strict, i.e. match strings exactly.</description>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="ValueExpressionType">
		<xsd:annotation>
			<xsd:documentation>
				<description>A component representing an expression of a query criteria used in a Get List. For example, find me the matching set of business objects where the Path Equals the Value. The Equals being the operator code, e.g. equals, less than, etc.</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element
				name="Value"
				type="cdt:TextType"
				minOccurs="0"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<description>The value to be matched in a query criteria. The repeating structure allows more than one value to be specified, these must be taken as an implict OR function, i.e. This Value-1 OR Value-2 OR Value-3.</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute
			name="operatorCode"
			type="tns:QueryOperatorCodeType">
			<xsd:annotation>
				<xsd:documentation>
					<description>The value from the codelist representing the operator to apply between the Path and the Value. Examples include Equals, Less Than, etc.</description>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute
			name="path"
			type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>
					<description>The path to the field within the business object to be used in the query. This field must use an XPath notation as per the MBP to provide a unique reference.</description>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="QueryExpressionType">
		<xsd:annotation>
			<xsd:documentation>
				<description>A query expression that allows complex queries to be formulated out of a series of value expressions (criteria).</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element
				name="ValueExpression"
				type="tns:ValueExpressionType"
				minOccurs="0"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<description>The value to be matched in a query criteria. The repeating structure allows more than one value to be specified, these must be taken as an implict OR function, i.e. This Value-1 OR Value-2 OR Value-3.</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute
			name="logicalOperatorCode"
			type="tns:LogicalOperatorCodeType">
			<xsd:annotation>
				<xsd:documentation>
					<description>A field indicating how consecutive query expression should be evaluated, e.g. Condition-1 AND Condition-2.</description>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute
			name="negationIndicator"
			type="xsd:boolean"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>
					<description>An indicator of whether the query expression should have a NOT applied to it, i.e. does NOT equal or is NOT less than.</description>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="SortExpressionType">
		<xsd:annotation>
			<xsd:documentation>A component holding the specification of the sorting required on the query.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element
				name="PathText"
				type="xsd:string"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The path by which to sort the result set returned from the query.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute
			name="sortOrderCode"
			type="tns:SortOrderCodeType"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>The direction by which to sort a set of values.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute
			name="collationAlgorithmCode"
			type="tns:CollationAlgorithmCodeType"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>The collation (sort) algorithm to use in the sorting to the result set</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	
	<xsd:complexType name="LocalDataType">
		<xsd:sequence>
			<xsd:element name="Entry" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType >
					<xsd:sequence>
						<xsd:element name="Value" type="xsd:token" minOccurs="1" maxOccurs="unbounded"/>
					</xsd:sequence>
					<xsd:attribute name="Key" type="xsd:token" use="required"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>