	</xsd:simpleType>


	<xsd:complexType name="MetricUoMLengthCodeType">
		<xsd:annotation>
			<xsd:documentation>A codelist restricting the values to metric (SI) lengths.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cdt:CodeType">
				<xsd:enumeration value="Centimetre" />
				<xsd:enumeration value="Kilometre" />
				<xsd:enumeration value="Metre" />
				<xsd:enumeration value="Micrometre" />
				<xsd:enumeration value="Milimetre" />
				<xsd:enumeration value="Nanometre" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:simpleType name="SimpleMetricUoMMassCodeType">
		<xsd:annotation>
			<xsd:documentation>A codelist restricting the values to metric (SI) mass.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Gram" />
