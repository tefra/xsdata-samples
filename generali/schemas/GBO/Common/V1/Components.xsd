	<xsd:complexType name="MaritalStatusCodeType">
		<xsd:annotation>
			<xsd:documentation>
				<description>A codelist indicating the marital status of an individual.</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cdt:CodeType">
				<xsd:enumeration value="Single" />
				<xsd:enumeration value="Married" />
				<xsd:enumeration value="Divorced" />
				<xsd:enumeration value="Widowed" />
				<xsd:enumeration value="Cohabiting" />
				<xsd:enumeration value="Civil Partnership" />
				<xsd:enumeration value="Domestic Partnership" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:simpleType name="SimpleMetricUoMLengthCodeType">
		<xsd:annotation>
			<xsd:documentation>A codelist restricting the values to metric (SI) lengths.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Centimetre" />
			<xsd:enumeration value="Kilometre" />
			<xsd:enumeration value="Metre" />
			<xsd:enumeration value="Micrometre" />
			<xsd:enumeration value="Milimetre" />
			<xsd:enumeration value="Nanometre" />
