<xsd:schema
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:cdt="http://generali.com/enterprise-services/core/gbo/common/core-types/v1"
	xmlns:tns="http://generali.com/enterprise-services/core/gbo/common/v1"
	targetNamespace="http://generali.com/enterprise-services/core/gbo/common/v1"
	elementFormDefault="qualified"
	attributeFormDefault="unqualified"
	version="1.0">
	
	<xsd:annotation>
		<xsd:documentation>
			<description>The schema containing the declarations of the common components.</description>
			<links>
				<link rel="self">./Components.xsd</link>
			</links>
		</xsd:documentation>
	</xsd:annotation>

    <!-- ++++++++++++++++++++++++++++++ -->
    <!--             Imports            -->
    <!-- ++++++++++++++++++++++++++++++ -->
	<xsd:import
		namespace="http://generali.com/enterprise-services/core/gbo/common/core-types/v1"
		schemaLocation="CoreTypes.xsd" />


    <!-- ++++++++++++++++++++++++++++++ -->
    <!--             Declarations       -->
    <!-- ++++++++++++++++++++++++++++++ -->
  
    <xsd:complexType name="CountryGBOType">
		<xsd:annotation>
			<xsd:documentation></xsd:documentation>
		</xsd:annotation>
			<xsd:sequence>
				<xsd:element
					name="ISOCode2"
					type="tns:ISOAlpha2CodeType"
					maxOccurs="1">
				</xsd:element>
				<xsd:element
					name="ISOCode3"
					type="tns:ISOAlpha3CodeType"
					maxOccurs="1" minOccurs="0">
				</xsd:element>
				<xsd:element name="name" type="cdt:TextType" minOccurs="0" />
				<xsd:element
					name="subdivisions"
					type="tns:SubdivisionsType"
					minOccurs="0" maxOccurs="1">
				</xsd:element>
			</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="SubdivisionsType">
		<xsd:sequence>
			<xsd:element name="subdivision" minOccurs="1" maxOccurs="unbounded" type="tns:SubDivisionType"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="SubDivisionType">
		<xsd:annotation>
			<xsd:documentation></xsd:documentation>
		</xsd:annotation>
			<xsd:sequence>
				<xsd:element
					name="subdivisionCode"
					type="xsd:token"
					maxOccurs="1" 
					minOccurs="0">
				</xsd:element>
				<xsd:element
					name="name"
					type="xsd:token"
					maxOccurs="1" 
					minOccurs="0">
				</xsd:element>
				<xsd:element
					name="subdivisionCategory"
					type="xsd:token"
					maxOccurs="1"
					minOccurs="0" >
				</xsd:element>
				<xsd:element
					name="subdivisions"
					type="tns:SubdivisionsType"
					minOccurs="0" 
					maxOccurs="1">
				</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:simpleType name="ISOAlpha2CodeType">
		<xsd:annotation>
			<xsd:documentation>ISO 3166-1 is part of the ISO 3166 standard published by the International Organization for Standardization (ISO), and defines codes for the names of countries, dependent territories, and special areas of geographical interest - ISO-Alpha2</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:pattern value="[A-Z][A-Z]" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="ISOAlpha3CodeType">
		<xsd:annotation>
			<xsd:documentation>ISO 3166-1 is part of the ISO 3166 standard published by the International Organization for Standardization (ISO), and defines codes for the names of countries, dependent territories, and special areas of geographical interest - ISO Alpha 3</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:pattern value="[A-Z][A-Z][A-Z]" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="StatusCodeType">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Active" />
			<xsd:enumeration value="Inactive" />
		</xsd:restriction>
	</xsd:simpleType>
    
    <xsd:simpleType name="DUNSType">
		<xsd:annotation>
			<xsd:documentation>The DUNS number pattern, the value should be like 000000001</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="9" />
			<xsd:pattern value="([0-9]{9})" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:complexType name="ClassificationType">
		<xsd:sequence>
			<xsd:element name="Code" type="cdt:CodeType"/>
			<xsd:element name="Type" type="tns:SICTypeEnum"/>
			<!-- Maybe this enum may become a code/value -->
			<xsd:element name="Description" type="cdt:TextType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Added as part of GER-3147 -->
	<xsd:complexType name="SicClassificationType">
		<xsd:sequence>
			<xsd:element name="Code" type="cdt:CodeType"/>
			<xsd:element name="Type" type="tns:SICTypeEnum"/>
			<xsd:element name="ShortDescription" type="cdt:TextType" minOccurs="0"/>
			<!-- Maybe this enum may become a code/value -->
			<xsd:element name="Description" type="cdt:TextType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:simpleType name="SICTypeEnum">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Division" />
			<xsd:enumeration value="MajorGroup" />
			<xsd:enumeration value="IndustryGroup" />
			<xsd:enumeration value="Industry" />
			<xsd:enumeration value="Sub-Industry" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:complexType name="GeographyType">
			<xsd:sequence>
				<xsd:element name="Description" type="cdt:TextType" minOccurs="0"></xsd:element>
				<xsd:element name="Name" type="cdt:CodeType"/>
				<xsd:element name="Country" type="tns:CountryGBOType" minOccurs="0"/>
				<xsd:element name="Address" minOccurs="0" >
					<xsd:complexType>
						<xsd:choice>
							<xsd:element name="Address" type="tns:AddressType"></xsd:element>
							<xsd:element name="GeocodedAddress" type="tns:GeoCodedAddressType"></xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
		
		<xsd:complexType name="AddressType">
			<xsd:sequence>
				<xsd:element name="AddressLine1" type="xsd:token"
					minOccurs="1" />
				<xsd:element name="AddressLine2" type="xsd:token"
					minOccurs="0" />
				<xsd:element name="State" type="xsd:token"
					minOccurs="0" />
				<xsd:element name="City" type="xsd:token" minOccurs="0" />
				<xsd:element name="ZipCode" type="xsd:token"
					minOccurs="0" />
				<xsd:element name="Country" type="tns:CountryGBOType"
					minOccurs="1" />
			</xsd:sequence>
		</xsd:complexType>
		
		<xsd:complexType name="GeoCodedAddressType">
			<xsd:complexContent>
				<xsd:extension base="tns:AddressType">
					<xsd:sequence>
						<xsd:element name="GeocodedAddresLine1" type="xsd:token" minOccurs="0" />
						<xsd:element name="GeocodedAddresLine2" type="xsd:token" minOccurs="0" />
						<xsd:element name="GeocodedState" type="xsd:token" minOccurs="0" />
						<xsd:element name="GeocodedTown" type="xsd:token" minOccurs="0" />
						<xsd:element name="GeocodedZipCode" type="xsd:token" minOccurs="0" />
						<xsd:element name="GeocodedCountry" type="tns:CountryGBOType" minOccurs="0" />
						<xsd:element name="GeocodedAccuracy" type="tns:GeocodedAccuracyEnum" minOccurs="0"/>
						<xsd:element name="GlobalPositionAddress" type="tns:GeographicLocationType" minOccurs="0"/>
						<xsd:element name="CrestaZone" type="tns:CrestaZoneType" minOccurs="0"/>
						<xsd:element name="AccuracyLevel" type="xsd:token" minOccurs="0"/> <!--  Added Accuracy Level 02/10/2018 -->
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
		
		<xsd:simpleType name="GeocodedAccuracyEnum">
			<xsd:restriction base="xsd:token">
				<xsd:enumeration value="Postcode"></xsd:enumeration>
				<xsd:enumeration value="Street"></xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleType>
		
		<xsd:complexType name="GlobalPositionAddressType">
			<xsd:sequence>
				<xsd:element name="Latitude" type="cdt:NumericType"/>
				<xsd:element name="Longitude" type="cdt:NumericType"/>
				<xsd:element name="WGSCode" type="cdt:IDType">
					<xsd:annotation>
					<xsd:documentation>The World Geodetic System (WGS) is a standard for use in cartography, geodesy, and navigation including by GPS. 
						It comprises a standard coordinate system for the Earth, a standard spheroidal reference surface (the datum or reference ellipsoid) for raw altitude data, 
						and a gravitational equipotential surface (the geoid) that defines the nominal sea level.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
		
		<xsd:complexType name="CrestaZoneType">
			<xsd:sequence>
				<xsd:element name="CrestaZoneIdentifier" type="cdt:IDType" />
				<xsd:element name="CrestaZoneName" type="cdt:IDType" minOccurs="0"/>
				<xsd:element name="CrestaSubZoneIdentifier" type="cdt:IDType" minOccurs="0"/>
				<xsd:element name="CrestaSubZoneName" type="cdt:IDType" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
		
		<xsd:complexType name="SMSNumberType">
		<xsd:annotation>
			<xsd:documentation>
				<description>
					<description>The definition for an SMS number.</description>
				</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element
				name="FullNumberText"
				type="cdt:TextType"
				minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<description>
							<description>The full SMS number.</description>
						</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="TelephoneNumberType">
		<xsd:annotation>
			<xsd:documentation>
				<description>A definition of a land line telephone number or MSISDN.</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element
				name="FullNumberText"
				type="cdt:TextType"
				minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<description>
							<description>The full telephone number, e.g. +44 0790 028 2454</description>
						</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element
				name="CountryCodeText"
				type="tns:TelephoneNumberCountryCodeType"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<description>
							<description>The Country Code of the Telephone Number, 1-3 digits.</description>
						</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element
				name="IdentificationCodeText"
				type="tns:TelephoneNumberIdentificationCodeType"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<description>
							<description>The Identification Code or Destination Code of the Telephone Number, 1-3 digits.</description>
						</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element
				name="SubscriberNumberText"
				type="tns:TelephoneNumberSubscriberNumberType"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<description>
							<description>The Subscriber Number of the Telephone Number, 1-3 digits.</description>
						</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="TelephoneNumberCountryCodeType">
		<xsd:annotation>
			<xsd:documentation>
				<description>
					<description>The definition Country Code part of a Telephone Number.</description>
				</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cdt:TextType">
            <!-- 
                <xsd:minLength value="1"/>
                <xsd:maxLength value="3"/>
            -->
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="TelephoneNumberSubscriberNumberType">
		<xsd:annotation>
			<xsd:documentation>
				<description>
					<description>The definition Subscriber Number part of a Telephone Number.</description>
				</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cdt:TextType" />
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="TelephoneNumberIdentificationCodeType">
		<xsd:annotation>
			<xsd:documentation>
				<description>
					<description>The definition Identification Code or Destination Code part of a Telephone Number.</description>
				</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cdt:TextType" />
		</xsd:simpleContent>
	</xsd:complexType>
	
	<xsd:complexType name="ThingActorType">
		<xsd:annotation>
			<xsd:documentation>A type defining the details of a thing, examples of thing include servers, vehicles, aircraft, etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:BaseComponentType">
				<xsd:sequence />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="EmailAddressType">
		<xsd:annotation>
			<xsd:documentation>
				<description>A Email Address conforming to RFC5321 and RFC5322.</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element
				name="FullAddressText"
				type="tns:EmailAddressFullType"
				minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<description>The full email address e.g. peter.jackson@vodafone.com</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element
				name="DomainPartText"
				type="tns:EmailAddressDomainPartType"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<description>The domain e.g. vodafone.com, without the @ symbol.</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element
				name="LocalPartText"
				type="tns:EmailAddressLocalPartType"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<description>The username e.g. peter.jackson</description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="EmailAddressFullType">
		<xsd:annotation>
			<xsd:documentation>
				<description>
					<description>The definition of the local part of an Email Address.</description>
				</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cdt:TextType">
				<xsd:pattern value=".*@.*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>


	<xsd:complexType name="EmailAddressLocalPartType">
		<xsd:annotation>
			<xsd:documentation>
				<description>The definition of the local part of an Email Address.</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cdt:TextType">
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="EmailAddressDomainPartType">
		<xsd:annotation>
			<xsd:documentation>
				<description>The definition of the domain part of an Email Address.</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cdt:TextType">
				<xsd:maxLength value="253" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="GeographicLocationType">
		<xsd:annotation>
			<xsd:documentation>
				<description>A geographic location, latitude and longitude</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:BaseSimpleComponentType">
				<xsd:sequence>
					<xsd:element
						name="AltitudeMeasure"
						type="cdt:MeasureType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<description>The measure of the altitude that reflects the vertical elevation of an object above a surface for this geographical coordinate (Reference ISO 6709</description>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element
						name="LatitudeMeasure"
						type="cdt:MeasureType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<description>The measure of the latitude as an angular distance north or south from the Equator meridian to the meridian of a specific place for this geographical coordinate. (Reference ISO 6709)</description>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element
						name="LongitudeMeasure"
						type="cdt:MeasureType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<description>The measure of the longitude as an angular distance east or west from the Greenwich meridian to the meridian of a specific place (Reference ISO 6709)</description>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element
						name="LatitudeDirectionCode"
						type="cdt:CodeType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<description>The indication of whether the latitude compass direction from the Equator meridian to the meridian of a specific place is North (+) or South (-). (Reference ISO 6709)</description>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element
						name="LongitudeDirectionCode"
						type="cdt:CodeType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<description>The indication of whether the longitude as a compass direction from the Greenwich meridian to the meridian of a specific place is East (-) or West (+) for this geographical coordinate. (Reference ISO 6709)</description>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element
						name="SystemID"
						type="cdt:IDType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<description>A unique identifier of the system used for measuring a geographical coordinate other than Global Positioning System</description>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="CodeDescriptionType">
		<xsd:sequence>
			<xsd:element name="Code" type="cdt:CodeType"/>
			<xsd:element name="Description" type="cdt:TextType" minOccurs="0"/>
			<!-- Adding Subtype to code description -->
			<xsd:element name="Subtype" type="cdt:TextType" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="GeographicalScope">
		<xsd:complexContent>
			<xsd:extension base="tns:CodeDescriptionType"></xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="ValueType">
		<xsd:choice>
			<xsd:element name="Amount" type="cdt:AmountType" />
			<xsd:element name="Percentage" type="cdt:PercentType" />
			<xsd:element name="Quantity" type="cdt:QuantityType" />
		</xsd:choice>
	</xsd:complexType>
	
	<!--  MPR-40 - Shrenika - 11/05/2020 - Changes added for new Deduction Type -->
	
	<xsd:complexType name="ValueTypeDeduction">
        <xsd:sequence>
            <xsd:element name="Percentage" type="cdt:PercentType" minOccurs="0"/>
            <xsd:element name="Amount" type="cdt:AmountType" minOccurs="0"/>
            <xsd:element name="Quantity" type="cdt:QuantityType" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    
	<xsd:complexType name="AmountOrQuantityType">
		
		<xsd:choice>
			<xsd:element name="Amount" type="cdt:AmountType" />
			<xsd:element name="Quantity" type="cdt:NumericType" />
		</xsd:choice>
	</xsd:complexType>
	
	<xsd:complexType name="PercValueType">
		<xsd:choice>
			<xsd:element name="Amount" type="cdt:AmountType" />
			<xsd:element name="Percentage" type="cdt:PercentType" />
		</xsd:choice>
	</xsd:complexType>
	
	<xsd:complexType name="BaseComponentType">
		<xsd:annotation>
			<xsd:documentation>
				<description>Base type for all business objects and components.</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute
			name="actionCode"
			type="xsd:token"
			use="optional">
			<xsd:annotation>
				<xsd:documentation>
					<description>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</description>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>


	<xsd:complexType name="BaseIdentifiedComponentType">
		<xsd:annotation>
			<xsd:documentation>
				<description>The base type for all business objects and components with an identifier and a lifecycle state.</description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:BaseComponentType">
				<xsd:sequence>
					<xsd:element
						name="IDs"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<description>The set of Identifiers for the business object or component. These should be used to uniquely identifier instances of the business object or component.</description>
							</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element
									name="ID"
									type="cdt:IDType"
									maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>
											<description>The identifier of the business object or component. This should be used to identify instances of a business object or component. Where the component is being used to reference another business object then this is the "primary key" of the target object. Use the attributes
												@schemeName and @schemeAgencyName to identify type of Identifier.</description>
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element
						name="NameText"
						type="cdt:TextType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<description>The name of the instance of a business object or component.</description>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element
						name="DescText"
						type="cdt:TextType"
						minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<description>A free text description of a business object or component.</description>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element
