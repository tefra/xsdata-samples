	</xsd:simpleType>

	<xsd:complexType name="MetricUoMMassCodeType">
		<xsd:annotation>
			<xsd:documentation>A codelist restricting the values to metric (SI) mass.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cdt:CodeType">
				<xsd:enumeration value="Gram" />
				<xsd:enumeration value="Kilogram" />
				<xsd:enumeration value="Milligram" />
				<xsd:enumeration value="Tonne" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:simpleType name="SimpleUoMTimeCodeType">
		<xsd:annotation>
			<xsd:documentation>A codelist restricting a unit of measure for a time value.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Day" />
			<xsd:enumeration value="Hour" />
			<xsd:enumeration value="Minute" />
			<xsd:enumeration value="Month" />
