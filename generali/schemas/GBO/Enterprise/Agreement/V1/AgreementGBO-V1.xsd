			<xsd:element name="Net" type="cdt:AmountType" minOccurs="0" />
			<xsd:element name="Technical" type="cdt:AmountType"
				minOccurs="0" />
			<xsd:element name="NumberOfInstallments"
				type="cdt:NumberType" />
			<xsd:element name="Description" type="cdt:TextType"
				minOccurs="0" />
			<xsd:element name="Identifier" type="cdt:IDType"
