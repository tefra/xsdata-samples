<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:cmn="http://generali.com/enterprise-services/core/gbo/common/v1"
	xmlns:cdt="http://generali.com/enterprise-services/core/gbo/common/core-types/v1"
	xmlns:tns="http://generali.com/enterprise-services/core/gbo/enterprise/agreement/v1"
	xmlns:ecmn="http://generali.com/enterprise-services/core/gbo/enterprise/common/v1"
	targetNamespace="http://generali.com/enterprise-services/core/gbo/enterprise/agreement/v1"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="1.0">
	
	<!-- v 1.1				RM      05/10/2018		- ESB-164: Added CountryOfBusiness.	
							AS		09/10/2018	 	ESB-174: 
																- Floater, Accuracy Level, AUNS Number added to Components XSD, 
																- isCentralisedPayment added to Global XSD
																- ClaimsTrigger added to Global XSD
																- Cover level fields added to Global XSD - InstructDate, DateSentToBroker, DatePolicyIssued, PaymentDate
																- CollectedAmounts representing Premium Collected Amount for each currency added to Global XSD
																- Documents Structure Added to Global XSD, Document Type Structure added to Components XSD.
							SS		14/11/2018		- ESB-176: Added ToPortalindicator
							SS		20/11/2018		- ESB-177 Added GWPShare, SharePercentage and NumberOfPTLayers
							SS		18/02/2019		- ESB-183 Added CountryOfRisk for CRM Submission
							SS		11/06/2019		- ESB-193 (and GER-2699) Added Documents at Program level (under ProgramGBOType)
							SS		01/08/2019		- GER-2767 - CAT Peril structure extended - Added new IndemnityType and Indemnity under LimitType; Added SeparateDeductible under DeductibleType
													- GER-2775 - Added ClearingHouseFlag 
													- ESB-201 - Added WorldwideFlag
							RM		21/08/2019		- GER-2820: Added StateRegionCode, StateRegionName, CountryOfPerilCode, CountryOfPerilName, ZipCodeOfPeril
		v2.1				RM		29/01/2020		- GER-2854: SanctionAndComplianceFlag
		v2.2				RM		07/05/2020		- Renamed from ClearingHouseIndicator to MPRIndicator
													- GER-2914: Added ReinsurancePolicyRef field within DocumentType in /Common/V1/Compponent.xsd.
							SS		11/05/2020		- MPR-40: Deduction Section Value Type change to - ValueTypeDeduction. Change also in GBO/Common/V1/Compponent.xsd.
							SS		16/07/2020		- GER-3147: Added 'Short Description' for SIC Code component and added Indemnity segment for Exposures (BI)
																(Change in Components.xsd and AgreementGBO-V1.xsd)
							SS		01/06/2021		- GER-3299: Added SequenceNumber for CoinsurerInvolvement segment
																Added ParentSequenceNumber for Reinsurers segment
																Added InsuranceFlows segment and its complex type
		v2.3				SS		02/09/2021		- Added Claims elements and Engineering fields at Program level
		v2.4				SS		13/10/2021		- Changed CountryOfRisk to mandatory
													  Added Engineering LOB related fields
													  Added Marine LOB related fields
													  Added 'SubType' to 'CodeDescriptionType'
													  Added CyberExclusion Fields
													  Added Pricing Fields
													  Added multiple fields including new TimeSpecification attribute in Perils
													  Also updated :
													  /GBO/Common/V1/Components.xsd
													  /GBO/Enterprise/Common/V1/Components.xsd
													  Added Extension Exposure under Pricing

													
-->	 
	<xsd:annotation>
		<xsd:documentation>
			<description>Business Object supporting retrieving a customer's
				information.</description>
		</xsd:documentation>
	</xsd:annotation>

	<!-- +++++++++++++++++++++++ -->
	<!-- Imports -->
	<!-- +++++++++++++++++++++++ -->

	<xsd:import
		namespace="http://generali.com/enterprise-services/core/gbo/common/core-types/v1"
		schemaLocation="../../../../GBO/Common/V1/CoreTypes.xsd" />

	<xsd:import
		namespace="http://generali.com/enterprise-services/core/gbo/common/v1"
		schemaLocation="../../../../GBO/Common/V1/Components.xsd" />

	<xsd:import
		namespace="http://generali.com/enterprise-services/core/gbo/enterprise/common/v1"
		schemaLocation="../../Common/V1/Components.xsd" />
		
	<xsd:include schemaLocation="AgreementInvolvementGBO-V1.xsd"></xsd:include>

	<!-- +++++++++++++++++++++++ -->
	<!-- Declarations -->
	<!-- +++++++++++++++++++++++ -->
	<xsd:element name="Program" type="tns:ProgramGBOType"/>
	
	<xsd:complexType name="AgreementType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseGBOType">
				<xsd:sequence>
					<xsd:element name="ExpiryDate" type="cdt:DateTimeType" />
    				<xsd:element name="InceptionDate" type="cdt:DateTimeType" />
    				<xsd:element name="Renewable" type="xsd:boolean" minOccurs="0"/>
    				<xsd:element name="IssueDate" type="cdt:DateTimeType" minOccurs="0"/>
    				<xsd:element name="LayerAttachmentPoint" type="cdt:AmountType" minOccurs="0"/>
    				<xsd:element name="LastRenewalDate" type="cdt:DateTimeType" minOccurs="0"/>
    				<xsd:element name="Endorsed" type="xsd:boolean" minOccurs="0" />
    				<xsd:element name="EndorsmentVersion" type="xsd:token" minOccurs="0" />
    				<xsd:element name="Status" type="xsd:token" />
    				<xsd:element name="StatusDate" type="cdt:DateTimeType" minOccurs="0"/>
					<xsd:element name="StatusNotes" type="cdt:TextType" minOccurs="0"/>
					<xsd:element name="SumInsured" type="cdt:AmountType" minOccurs="0"/>
					<xsd:element name="TotalLayers" type="cdt:NumberType" minOccurs="0"/>
    				<xsd:element name="RenewedFrom" type="cdt:IDType" minOccurs="0"/>
    				<xsd:element name="UnderwritingYear" type="xsd:nonNegativeInteger" />
					<xsd:element name="Version" type="xsd:nonNegativeInteger" />
					<xsd:element name="AccountManagers" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="AccountManager" type="tns:AccountManagerInvolvementType" minOccurs="1" maxOccurs="unbounded" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="BusinessOrigin" type="tns:BusinessOriginEnum" minOccurs="0"/>
					<xsd:element name="ClassificationCode" type="tns:ClassificationCodeEnum" minOccurs="0"></xsd:element>
					<xsd:element name="CoinsurerInvolvements" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Coinsurer" type="tns:CoinsurerInvolvementType" maxOccurs="unbounded" minOccurs="0"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="Contacts" type="tns:ContactsInvolvement" minOccurs="0"/>
					<xsd:element name="InsuredInvolvement" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Insured" type="tns:InsuredInvolvement" maxOccurs="unbounded" minOccurs="0"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<!--  fast change for backward compatibility, future will have better one -->
					<xsd:element name="Intermediary"
						type="tns:IntermediaryInvolvementType" minOccurs="0" maxOccurs="unbounded"/>
					
					<xsd:element name="Underwriters" minOccurs="0">	
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Underwriter" type="tns:UnderwriterInvolvement" minOccurs="1" maxOccurs="unbounded"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					
					<xsd:element name="LayerType" type="tns:LayerTypeEnum" minOccurs="1">
					</xsd:element>
					<xsd:element name="LineOfBusiness" type="ecmn:LineOfBusinessType" />
					<xsd:element name="PayoutBenefits" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="PayoutBenefit" type="tns:PayoutBenefit" minOccurs="0" maxOccurs="unbounded"></xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					
					<xsd:element name="LocalCurrency" type="cdt:CodeType" minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>The currency code, should be an ISO 3 digit code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="RemittanceCurrency" type="cdt:CodeType" minOccurs="1">
						<xsd:annotation>
							<xsd:documentation>The currency code, should be an ISO 3 digit code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>				
					<xsd:element name="LocalData" type="cmn:LocalDataType" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>This element contains a key value implementation for local data tab.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	
	   <xsd:complexType name="ProgramGBOType">
    	<xsd:complexContent>
    		<xsd:extension base="tns:AgreementType">
    			<xsd:sequence>
    				<xsd:element name="LongTermAgreement"
    					type="xsd:boolean" minOccurs="0" />
    				<xsd:element name="LongTermStartDate"
    					type="cdt:DateTimeType" minOccurs="0" />
    				<xsd:element name="LongTermEndDate"
    					type="cdt:DateTimeType" minOccurs="0" />
    				<xsd:element name="OnRiskSince"
    					type="cdt:DateTimeType" minOccurs="0" />
    				<xsd:element name="RenewalDate"
    					type="cdt:DateTimeType" minOccurs="0" />
    				<xsd:element name="YearSequenceNumber"
    					type="xsd:nonNegativeInteger" />
    				<xsd:element name="Producer"
    					type="tns:ProducerInvolvementType" minOccurs="1" />
    				<xsd:element name="CancellationDaysNotice"
    					type="cdt:NumberType" />
    				<xsd:element name="Risks" minOccurs="0"
    					maxOccurs="1">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="Risk"
    								type="tns:RiskType" minOccurs="0" maxOccurs="unbounded">
    							</xsd:element>
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>
    				<xsd:element name="Covers" minOccurs="1">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="Cover"
    								type="tns:CoverGBOType" minOccurs="0" maxOccurs="unbounded" />
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>
    				<xsd:element name="Deductibles"
    					type="tns:DeductiblesType" minOccurs="0" />
    				<xsd:element name="Limits" type="tns:LimitsType"
    					minOccurs="0" />
    				<xsd:element name="PremiumFactors" minOccurs="0">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="PremiumFactor"
    								type="tns:PremiumFactorType" minOccurs="0"
    								maxOccurs="unbounded" />
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>
    				<xsd:element name="Perils"
    					type="tns:PerilsStructureType" minOccurs="0" />
    				<!-- Changed the SicCode type as part of GER-3147 -->
    				<xsd:element name="SiCCode"
    					type="cmn:SicClassificationType" minOccurs="0" />
    				<!--  this should be one, but  right now GeR doesn't provide it, if not present use AccountSicCode-->
    				<xsd:element name="LegalCost"
    					type="tns:LegalCostType" minOccurs="0" />
    				<!--  this element is mandatory for Casualty and Financial line (mainly where liability are involved) -->
    				<xsd:element name="TotalPremium"
    					type="tns:TotalPremiumType" minOccurs="0" />
    				<!--  added for Program level premium -->
    				<xsd:element name="ProgramLimitOfIndemnity"
    					type="cdt:AmountType" minOccurs="0" />
    				<xsd:element name="PharmaceuticalRisk"
    					type="xsd:boolean" minOccurs="0" />
    				<xsd:element name="IsCentralisedPremiumPayment"
    					type="xsd:boolean" minOccurs="0" />
    				<!--  ESB-174: C&B Project: Added isCentralisedPremiumPayment at Program Level -->
    				<xsd:element name="ToPortalIndicator"
    					type="cdt:TextType" minOccurs="0" />
    				<!--  C&B Portal included to filter/route messages -->
    				<xsd:element name="Documents" minOccurs="0">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="Document"
    								type="ecmn:DocumentType" minOccurs="0"
    								maxOccurs="unbounded" />
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>
    				<xsd:element name="MPRIndicator" type="xsd:boolean" minOccurs="0"></xsd:element> <!-- renamed ClearingHouseFlat to MPRIndicator -->
    				<!-- v5.13 GER-2854: SanctionAndComplianceFlag -->
    				<xsd:element name="SanctionAndComplianceFlag" type="xsd:boolean" minOccurs="0"></xsd:element>
					<!-- v2.4 Adding Pricing Segment -->
					<xsd:element name="Pricing" type="tns:PricingType" minOccurs="0" />
					<!-- v2.4 Adding Cyber Exclusion Fields -->
					<xsd:element name="CyberExclusion" type="tns:CyberExclusionType" minOccurs="0" />
					<!-- v2.3 Adding fields for Claims and Engineering specific data -->
					<xsd:element name="FamilySubmissionReference" type="xsd:string"  minOccurs="0"/>
					<xsd:element name="CarSubmissionReference" type="xsd:string" minOccurs="0" />
					<xsd:element name="ProjectTypeCode" type="xsd:string" minOccurs="0" />
					<xsd:element name="PublishClaimsWithPaidValues" type="xsd:boolean"  minOccurs="0" />
					<xsd:element name="PublishClaimsWithoutPaidValues" type="xsd:boolean"  minOccurs="0" />
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    
	<xsd:simpleType name="LayerTypeEnum">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Primary"/>
			<xsd:enumeration value="Excess"/>
			<xsd:enumeration value="QuotaShare"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="BusinessOriginEnum">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="NewBusiness" />
			<xsd:enumeration value="Renewal" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="ClassificationCodeEnum">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Multinational" />
			<xsd:enumeration value="Mononational" />
		</xsd:restriction>
	</xsd:simpleType>

	<!-- v2.4 Adding Cyber Exclusion Complex Type -->
	<xsd:complexType name="CyberExclusionType">
		<xsd:sequence>
			<xsd:element name="Excluded" type="xsd:boolean" minOccurs="0" />
			<xsd:element name="Type" type="xsd:string" minOccurs="0" />
			<xsd:element name="CoverType" type="xsd:string" minOccurs="0" />
			<xsd:element name="Limit" type="tns:LimitType" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>

	<!-- v2.4 Start : Adding Pricing Complex Types -->
	<xsd:complexType name="PricingType">
		<xsd:sequence>
			<xsd:element name="InvolvementType" type="xsd:string" minOccurs="0" />
			<xsd:element name="ProducingCountryName" type="xsd:string" minOccurs="0" />
			<xsd:element name="Layers" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Layer" type="tns:PricingLayerType" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Deductions" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Deduction" type="tns:PricingDeductionType" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Extensions" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Extension" type="tns:PricingExtensionType" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Deductibles" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Deductible" type="tns:PricingInterestType" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Limits" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Limit" type="tns:PricingInterestType" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="PricingAmounts" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="PricingAmount" type="tns:PricingAmountType" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="PricingLayerType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element name="Primary" type="xsd:boolean" minOccurs="0" />
					<xsd:element name="AttachmentPoint" type="cdt:NumericType" minOccurs="0" />
					<xsd:element name="Limit" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Value" type="cmn:ValueType" minOccurs="0" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="POShare" type="cdt:NumericType" minOccurs="0" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="PricingDeductionType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element name="DeductionType" type="cmn:CodeDescriptionType" minOccurs="0" />
					<xsd:element name="Value" type="cmn:ValueType" minOccurs="0" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="PricingExtensionType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element name="Code" type="xsd:string" minOccurs="0" />
					<xsd:element name="RateBasis" type="xsd:string" minOccurs="0" />
					<xsd:element name="RatePC" type="cdt:NumericType" minOccurs="0" />
					<xsd:element name="Exposure" type="cdt:NumericType" minOccurs="0" />
					<xsd:element name="Sublimit" type="cdt:NumericType" minOccurs="0" />
					<xsd:element name="Premium" type="cdt:NumericType" minOccurs="0" />
					<xsd:element name="Comment" type="xsd:string" minOccurs="0" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="PricingInterestType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element name="AmountTypes" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="AmountType" type="tns:PricingInterestAmountType" minOccurs="0" maxOccurs="unbounded" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="PricingInterestAmountType">
		<xsd:sequence>
			<xsd:element name="Value" type="cmn:ValueType" minOccurs="0" />
			<xsd:element name="Type" type="xsd:string" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="PricingAmountType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element name="Premium" type="tns:PricingPremiumType" minOccurs="0" />
					<xsd:element name="ActualCommission" type="cdt:NumericType" minOccurs="0" />
					<xsd:element name="GeneraliGwpShare" type="cdt:NumericType" minOccurs="0" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="PricingPremiumType">
		<xsd:sequence>
			<xsd:element name="Gross" type="cdt:NumericType" minOccurs="0" />
			<xsd:element name="Actual" type="cdt:NumericType" minOccurs="0" />
			<xsd:element name="MinimumAcceptable" type="cdt:NumericType" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- v2.4 End : Adding Pricing Complex Type -->
	
	<xsd:complexType name="LimitType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element name="Value" type="cmn:ValueType" />
					<xsd:element name="Maximum" type="cdt:AmountType"
						minOccurs="0" />
					<xsd:element name="Minimum" type="cdt:AmountType"
						minOccurs="0" />
					<xsd:element name="Basis"
						type="cmn:CodeDescriptionType" />
					<!-- v2.4 Added TimeSpecification Attribute -->
					<xsd:element name="TimeSpecification" type="cdt:NumericType" 
					minOccurs="0" />
					
					<xsd:element name="RestrictedToGeography"
						minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Geography"
									type="cmn:GeographyType" minOccurs="0" maxOccurs="unbounded" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="ValueAppliesTo"
						type="cmn:CodeDescriptionType" />
					<xsd:element name="Type">
						<xsd:simpleType>
							<xsd:restriction base="xsd:token">
								<xsd:enumeration value="Main"></xsd:enumeration>
								<xsd:enumeration value="SubLimit"></xsd:enumeration>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="Indemnity" type="tns:IndemnityType" minOccurs="0"></xsd:element>
				</xsd:sequence>
				<xsd:attribute name="Grouped" default="false" type="xsd:boolean" />				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="LimitsType">
		<xsd:sequence>
			<xsd:element name="Limit" type="tns:LimitType" minOccurs="0"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="ClausesType">
		<xsd:sequence>
			<xsd:element name="Clause" type="tns:ClauseType"
				minOccurs="0" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="DeductibleType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element name="Value" type="cmn:ValueType"
						minOccurs="0" />
					<xsd:element name="Basis"
						type="cmn:CodeDescriptionType" minOccurs="0" />
					<xsd:element name="ValueAppliesTo"
						type="cmn:CodeDescriptionType" minOccurs="0" />
					<xsd:element name="RestrictedToGeography"
						minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Geography"
									type="cmn:GeographyType" minOccurs="0" maxOccurs="unbounded" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="DeductibleType" default="Main"
						minOccurs="1">
						<xsd:simpleType>
							<xsd:restriction base="xsd:token">
								<xsd:enumeration value="Main" />
								<xsd:enumeration
									value="PerilPolicyDeductible" />
								<xsd:enumeration
									value="PerilRiskDeductible" />
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="Maximum" type="cdt:AmountType"
						minOccurs="0" />
					<xsd:element name="Minimum" type="cdt:AmountType"
						minOccurs="0" />
					<xsd:element name="SeparateDeductible" type="xsd:boolean" minOccurs="0"></xsd:element>
				</xsd:sequence>
				<xsd:attribute name="Grouped" default="false" type="xsd:boolean" />	
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="AppliedDeductionsType">
