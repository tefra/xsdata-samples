		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="OnRenewal" />
			<xsd:enumeration value="OnExpiry" />
			<xsd:enumeration value="FirstInstalment" />
			<xsd:enumeration value="LastInstalment" />
			<xsd:enumeration value="SkipFirstInstalment" />
			<xsd:enumeration value="EquallyAcrossAllInstalment" />
