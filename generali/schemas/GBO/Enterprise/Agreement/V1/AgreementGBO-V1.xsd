	<xsd:complexType name="ClauseType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element name="Code" type="cdt:CodeType"  minOccurs="0"/>
					<xsd:element name="Wording" type="cdt:TextType" minOccurs="0"/>
                    <xsd:element name="Limits" type="tns:LimitsType" minOccurs="0"/>
                    <xsd:element name="Deductibles" type="tns:DeductiblesType" minOccurs="0" />
				</xsd:sequence>
				<xsd:attribute name="IsIncluded" type="xsd:boolean" />
				<xsd:attribute name="IsCustom" type="xsd:boolean" />
				<xsd:attribute name="IsExtension" type="xsd:boolean" default="false" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
    
    <xsd:complexType name="PremiumFactorType">
    	<xsd:complexContent>
    		<xsd:extension base="cmn:BaseComponentType">
    			<xsd:sequence>
    				<xsd:element name="Amount" type="cdt:AmountType" />
    				<xsd:element name="Description" type="cdt:TextType" />
    				<xsd:element name="Percentage" type="cdt:PercentType" />
    				
    				
    			</xsd:sequence>
    		</xsd:extension>
    		
    	</xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="CoverGBOType">
    	<xsd:complexContent>
    		<xsd:extension base="tns:AgreementType">
    			<xsd:sequence>
    				<xsd:element name="DeclarationNumber"
    					type="cdt:IDType" minOccurs="0" />
    				<xsd:element name="ClaimJurisdiction"
    					type="cdt:TextType" />
    				<xsd:element name="CostInAddition"
    					type="xsd:boolean" default="false" />
    				<xsd:element name="PolicyJurisdiction"
    					type="cdt:TextType" />
    				<xsd:element name="Layer" type="cdt:NumberType"
    					minOccurs="0" />
    				<xsd:element name="Share" type="cdt:PercentType"
    					minOccurs="0" />
    				<xsd:element name="Visible" type="xsd:boolean" />
    				<xsd:element name="CountryOfBusiness"
    					type="cmn:ISOAlpha2CodeType" minOccurs="0" />
    				<!-- Added as part of ESB-164 -->
    				<xsd:element name="Cedants" minOccurs="0">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="Cedant"
    								type="tns:CedantInvolvementType" minOccurs="0"
    								maxOccurs="unbounded" />
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>
    				<xsd:element name="Front" minOccurs="0"
    					type="tns:FrontingInvolvementType" />
    				<xsd:element name="Reinsurers" minOccurs="0">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="Reinsurer"
    								maxOccurs="unbounded" type="tns:ReinsurerInvolvementType" />
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>
					
					<!-- GER-3299 - Added InsuranceFlows segment -->
					<xsd:element name="InsuranceFlows" minOccurs="0">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="InsuranceFlow"
    								maxOccurs="unbounded" type="tns:InsuranceFlowType" />
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>

    				<xsd:element name="Risks" minOccurs="0"
    					maxOccurs="1">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="Risk"
    								type="tns:RiskType" minOccurs="0" maxOccurs="unbounded">
    							</xsd:element>
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>
					
					<!-- v2.4 Added Marine Vessel segment -->
					<xsd:element name="Vessels" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element type="tns:VesselType" name="Vessel" minOccurs="0" maxOccurs="unbounded" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
    				
					<xsd:element name="PolicyType"
    					type="tns:PolicyTypeEnum" />
    				<xsd:element name="Coverages"
    					type="tns:CoveragesType" />
    				<xsd:element name="PolicyHolderInvolvement"
    					type="tns:PolicyholderInvolvement" minOccurs="0" />
    				<xsd:element name="Deductibles"
    					type="tns:DeductiblesType" minOccurs="0" />
    				<xsd:element name="Deductions"
    					type="tns:DeductionsType" />
    				<xsd:element name="TotalDeductions"
    					type="tns:TotalDeductionsType" />
    				<xsd:element name="Extensions"
    					type="tns:ExtensionsType" minOccurs="0" />

    				<xsd:element name="Limits" type="tns:LimitsType"
    					minOccurs="0" />
    				<xsd:element name="Exposures"
    					type="tns:ExposuresType" minOccurs="0">
    				</xsd:element>
    				<!--
    					Removed as for Oliver input 
    					<xsd:element name="PerilStructure" type="tns:PerilsType" minOccurs="0"/>
    				-->
    				<xsd:element name="Premiums">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="Premium"
    								type="tns:PremiumType" minOccurs="0" maxOccurs="unbounded" />
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>
    				<xsd:element name="TotalPremium"
    					type="tns:TotalPremiumType" minOccurs="0" />
    				<xsd:element name="Clauses" type="tns:ClausesType"
    					minOccurs="0" />
    				<xsd:element name="ExchangeRate"
    					type="cdt:NumericType" minOccurs="0" default="1.00" />
    				<xsd:element name="IsCentralisedPremiumPayment"
    					type="xsd:boolean" minOccurs="0" />
    				<!--  ESB-174: C&B Project Added isCentralisedPremiumPayment at Cover Level -->

    				<!--  ESB-174: C&B Project : Added below fields-->

    				<xsd:element name="InstructDate"
    					type="cdt:DateTimeType" minOccurs="0" />
    				<xsd:element name="DateSentToBroker"
    					type="cdt:DateTimeType" minOccurs="0" />
    				<xsd:element name="DatePolicyIssued"
    					type="cdt:DateTimeType" minOccurs="0" />
    				<xsd:element name="PaymentDate"
    					type="cdt:DateTimeType" minOccurs="0" />

    				<!--  ESB-174: C&B Project : Documents Structure added here  -->

    				<xsd:element name="Documents" minOccurs="0">
    					<xsd:complexType>
    						<xsd:sequence>
    							<xsd:element name="Document"
    								type="ecmn:DocumentType" minOccurs="0"
    								maxOccurs="unbounded" />
    						</xsd:sequence>
    					</xsd:complexType>
    				</xsd:element>
    				<xsd:element name="CountryOfRisk" type="cmn:ISOAlpha2CodeType"></xsd:element>
					<!-- v2.3 Adding new segment for Claims	-->
					<xsd:element name="Claims" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Claim" type="tns:ClaimType" minOccurs="0" maxOccurs="unbounded" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<!-- v2.4 Adding new segment for Engineering Fields -->
					<xsd:element name="Engineering" type="tns:EngineeringType" minOccurs="0" />
    			</xsd:sequence>
    		</xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
	
	<!-- v2.3 Adding new complex type for Claims	-->
	
