    <xsd:complexType name="PayoutBenefit">
    	<xsd:complexContent>
    		<xsd:extension base="cmn:BaseIdentifiedComponentType"></xsd:extension>
    	</xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ExtensionsType">
    	<xsd:sequence>
    		<xsd:element name="Extension" type="tns:ExtensionType" minOccurs="0" maxOccurs="unbounded"/>
    	</xsd:sequence>
    </xsd:complexType>
    
    
    <xsd:complexType name="ExtensionType">
    	<xsd:sequence>
    		<xsd:element name="EffectiveDate" type="cdt:DateTimeType" minOccurs="0"/>
    		<xsd:element name="EndDate" type="cdt:DateTimeType" minOccurs="0"/>
    		<xsd:element name="RetroactiveDate" type="cdt:DateTimeType" minOccurs="0" />
    		<xsd:element name="TermDate" type="cdt:DateTimeType" minOccurs="0"/>
    		<xsd:element name="Premium" type="tns:PremiumType" minOccurs="0"/>
    		<xsd:element name="Exposures" type="tns:ExposuresType" minOccurs="0" />
    		<xsd:element name="Deductibles" type="tns:DeductiblesType" minOccurs="0" />
    		<xsd:element name="Clauses" type="tns:ClausesType" minOccurs="0" />
    		<xsd:element name="Limits" type="tns:LimitsType" minOccurs="0" />
    		<xsd:element name="ExtensionCode" type="cmn:CodeDescriptionType"  />
    		<xsd:element name="GeographicalScope" type="cmn:GeographicalScope" minOccurs="0"/>
    	</xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="ExposuresType">
		<xsd:sequence>
			<xsd:element name="Exposure" type="tns:ExposureType" minOccurs="0" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="ExposureType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element name="AnnualRate" type="cdt:AmountType" />
					<xsd:element name="EstimatedValue" type="cmn:AmountOrQuantityType" />
