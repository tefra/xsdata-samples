					<!-- <xsd:element name="ExposureSpecification" type="ecmn:ExposureSpecificationType" minOccurs="0"/> -->
					<xsd:element name="Premium" type="tns:PremiumType" minOccurs="0"/>
					<xsd:element name="CalculationFactor" type="cdt:NumericType" minOccurs="0" />
					<xsd:element name="ApplyRateType">
						<xsd:simpleType>
							<xsd:restriction base="xsd:token">
								<xsd:enumeration value="Rate" />
								<xsd:enumeration value="Flat" />
								<xsd:enumeration value="PerMills" />
								<xsd:enumeration value="Unit"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					
					<xsd:element name="Unit" type="cmn:CodeDescriptionType" minOccurs="0"/>
					<xsd:element name="Item" type="cmn:CodeDescriptionType" minOccurs="0"/>
					<xsd:element name="Basis" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:token">
								<xsd:enumeration value="AverageValue"/>
								<xsd:enumeration value="Percentage"/>
								<xsd:enumeration value="ActualValue"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="CalculationRateType" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:token">
								<xsd:enumeration value="AverageValue"/>
								<xsd:enumeration value="Percentage"/>
								<xsd:enumeration value="ActualValue"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="Coverage" type="tns:CoverageType" minOccurs="0" />
					<xsd:element name="MFL" type="cmn:PercValueType" minOccurs="0" />
					<xsd:element name="UnitPrice" type="cdt:AmountType" minOccurs="0" />
					<!--  Added as part of GER-3147 -->
					<xsd:element name="Indemnity" type="tns:IndemnityType" minOccurs="0"></xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="RiskType">
    	<xsd:sequence>
			<xsd:element name="RiskElement" type="tns:RiskElementInvolvementType"/>
			
