	<!-- v2.4 Start: Adding 3 Engineering related complex type, 1 Simple Type and 2 Vessel Types for Marine -->
	<xsd:complexType name="EngineeringType">
		<xsd:sequence>
			<xsd:element type="cdt:DateTimeType" name="SignoffDate" minOccurs="0" />
			<xsd:element type="cdt:NumberType" name="TestCommissionDays" minOccurs="0" />
			<xsd:element type="tns:EngineeringMaintenanceType" name="MaintenanceRecord" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="EngineeringMaintenanceType">
		<xsd:sequence>
			<xsd:element type="cdt:DateTimeType" name="InceptionDate" />
			<xsd:element type="cdt:DateTimeType" name="ExpiryDate" />
			<xsd:element name="Years">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Maintenance" type="tns:MaintenanceType" minOccurs="1" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="MaintenanceType">
		<xsd:sequence>
			<xsd:element type="cdt:NumberType" name="Year" />
			<xsd:element type="tns:MaintenanceTypeEnum" name="Type" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:simpleType name="MaintenanceTypeEnum">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Visit"/>
			<xsd:enumeration value="Extended"/>
			<xsd:enumeration value="Guarantee"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:complexType name="VesselType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseIdentifiedComponentType">
				<xsd:sequence>
					<xsd:element type="xsd:string" name="ImoNumber" minOccurs="0"/>
					<xsd:element type="xsd:boolean" name="Top" minOccurs="0"/>
					<xsd:element type="xsd:string" name="Flag" minOccurs="0"/>
					<xsd:element type="xsd:string" name="Name" minOccurs="0"/>
					<xsd:element type="xsd:string" name="Segment" minOccurs="0"/>
					<xsd:element type="cdt:NumericType" name="GrossTonnage" minOccurs="0"/>
					<xsd:element type="cdt:NumericType" name="DWT" minOccurs="0"/>
					<xsd:element type="cdt:NumberType" name="Score" minOccurs="0"/>
					<xsd:element type="xsd:string" name="ClassificationSociety" minOccurs="0"/>
					<xsd:element type="xsd:nonNegativeInteger" name="BuiltYear" minOccurs="0"/>
					<xsd:element name="VesselInterests" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element type="tns:VesselInterestType" name="VesselInterest"
									minOccurs="0" maxOccurs="unbounded"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="VesselInterestType">
		<xsd:sequence>
			<xsd:element type="xsd:string" name="TemplateName" minOccurs="0"/>
			<xsd:element type="xsd:string" name="Currency" minOccurs="0"/>
			<xsd:element type="cdt:AmountType" name="Value" minOccurs="0"/>
			<xsd:element type="cdt:NumericType" name="Rate" minOccurs="0"/>
			<xsd:element type="cdt:AmountType" name="Deductible" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	
