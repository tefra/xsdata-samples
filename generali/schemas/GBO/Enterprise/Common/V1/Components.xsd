	<xsd:complexType name="PerilType">
		<xsd:complexContent>
			<xsd:extension base="cmn:CodeDescriptionType"/>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="SpecificationBaseType">
		<xsd:complexContent>
			<xsd:extension base="cmn:CodeDescriptionType"/>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="ExposureItemType">
		<xsd:complexContent>
			<xsd:extension base="cmn:CodeDescriptionType"/>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="PremiumAllocationType">
		<xsd:sequence>
			<xsd:element type="cmn:ISOAlpha2CodeType" name="CountryCode" />
			<xsd:element type="cdt:AmountType" name="BeginningOfYear" />
			<xsd:element type="cdt:AmountType" name="BeginningOfYearLocalShare" />
		</xsd:sequence>
	</xsd:complexType>
	
	<!--  Added Common Document Type Structure -->
	
	 <xsd:complexType name="DocumentType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseReferenceComponentType">
				<xsd:sequence>
					<!-- We already have CreatedBy in the BaseIdentifiedComponentType. This represents Author -->
					<!-- We already have CreationDate and DateLastUpdate  in the BaseIdentifiedComponentType -->
					<!-- We already have a name in the BaseIdentifiedComponentType. This can be used for title -->
					<!-- Type will be part of BaseReferenceComponentType -->
					<xsd:element name="Note" type="xsd:string"
						minOccurs="0" /><!-- Only occurs if DocTabType is Policy Wordings-->
					<xsd:element name="DocumentLocation"
						type="xsd:anyURI" minOccurs="0" />
					<xsd:element name="FileType" type="xsd:string"
						minOccurs="0" />
					<xsd:element name="Author" type="cdt:TextType"
						minOccurs="0">
					</xsd:element>
					<xsd:element name="DocType" type="cdt:TextType"
						minOccurs="0">
					</xsd:element>
					<xsd:element name="LocalPolicyRef"
						type="cdt:TextType" minOccurs="0">
					</xsd:element>
					<xsd:element name="IssueDate"
						type="cdt:DateTimeType" minOccurs="0">
					</xsd:element>
					<xsd:element name="ProducingOfficeRef"
						type="cdt:TextType" minOccurs="0">
					</xsd:element>
					<xsd:element name="AdditionalInformation" type="cdt:TextType" minOccurs="0"></xsd:element>
					<xsd:element name="CDMSDocumentLocation"
						type="xsd:anyURI" minOccurs="0" />
					<xsd:element name="ReinsurancePolicyRef" type="cdt:TextType" minOccurs="0"></xsd:element>	
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!--  Added Common PaymentTransaction Structure -->
	
	 <xsd:complexType name="PaymentTransactionType">
		<xsd:complexContent>
			<xsd:extension base="cmn:BaseReferenceComponentType">
				<xsd:sequence>
					<xsd:element name="PaymentDate" type="cdt:DateTimeType" minOccurs="0"/>
					<xsd:element name="PaymentDueDate" type="cdt:DateTimeType" minOccurs="0"/>
					<xsd:element name="Amount" type="cdt:AmountType" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>	

</xsd:schema>
