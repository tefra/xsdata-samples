        </ns1:Code>
        <ns1:Code id="CA">
          <ns2:Name xmlns:ns2="http://www.sdmx.org/resources/sdmxml/schemas/v3_1/common">Canada</ns2:Name>
        </ns1:Code>
        <ns1:Code id="EU">
