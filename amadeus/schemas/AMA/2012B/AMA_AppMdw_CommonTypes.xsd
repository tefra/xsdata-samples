	<xs:complexType name="AddressType">
		<xs:annotation>
			<xs:documentation>Address is a generic type to convey a phone number,
				an email address...
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Format" type="String_Length1To3" use="required">
			<xs:annotation>
				<xs:documentation>format of the recipient address
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Value" type="String_Length1To500" use="required"/>
	</xs:complexType>
	<xs:complexType name="AddressWithModeType">
		<xs:complexContent>
			<xs:extension base="AddressType">
				<xs:attribute name="Mode" type="String_Length1To3" use="optional">
					<xs:annotation>
						<xs:documentation>CRT or PNT when a TID is used. TO, CC, or BCC
							when an EML address is used.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OwnershipType">
		<xs:annotation>
			<xs:documentation>Owner of the Query, containing: organization
				(mandatory), application (mandatory)
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="OrganizationCode" type="String_Length1To10" use="required"/>
		<xs:attribute name="ApplicationLabel" type="String_Length1To10" use="required"/>
		<xs:attribute name="CompanyCode" type="String_Length1To10" use="optional"/>
	</xs:complexType>
	<xs:complexType name="TextType">
		<xs:attribute name="Encoding" type="String_Length1To10" use="required"/>
		<xs:attribute name="Value" type="String_Length1To199" use="required"/>
	</xs:complexType>
	<xs:complexType name="FTP_Type">
		<xs:attribute name="Filename" type="String_Length1To199" use="required"/>
		<xs:attribute name="Path" type="String_Length1To199" use="required"/>
		<xs:attribute name="FileHash" type="String_Length1To100" use="optional"/>
		<xs:attribute name="FTPID" type="String_Length1To25" use="optional"/>
	</xs:complexType>
	<xs:complexType name="FTP_TypeWithLayoutType">
		<xs:complexContent>
			<xs:extension base="FTP_Type">
				<xs:attribute name="LayoutType" type="String_Length1To5" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LocationType">
		<xs:annotation>
			<xs:documentation>Location of a device</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Location">
				<xs:annotation>
					<xs:documentation>Global location of the device (airport, city, etc.)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="Airport">
							<xs:annotation>
								<xs:documentation>Airport location description</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attributeGroup ref="ota:AirportLocationGroup"/>
								<xs:attribute ref="BuildingCode">
									<xs:annotation>
										<xs:documentation>Code used to identify a building</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="City">
							<xs:annotation>
								<xs:documentation>City location description</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attributeGroup ref="ota:LocationGroup"/>
								<xs:attribute ref="BuildingCode">
									<xs:annotation>
										<xs:documentation>Code used to identify a building</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="LocationCategory" type="CategoryType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Detailed description of the location</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:attribute name="BuildingCode" type="ota:StringLength1to8"/>
	<xs:complexType name="CategoryType">
		<xs:attributeGroup ref="ama_ct:CodeGroup"/>
		<xs:attribute name="IndexNumber" type="ota:StringLength1to8"/>
	</xs:complexType>
	<xs:complexType name="ApplicationType">
		<xs:annotation>
			<xs:documentation>Application identification</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ApplicationLabel" type="ota:StringLength1to64" use="required"/>
		<xs:attribute name="IndexNumber" type="ota:NumericStringLength1to8"/>
	</xs:complexType>
</xs:schema>
