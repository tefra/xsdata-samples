<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.travelport.com/schema/sharedUprofile_v20_0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.travelport.com/schema/sharedUprofile_v20_0"
           xmlns:common="http://www.travelport.com/schema/uprofileCommon_v30_0" 
           elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:annotation>
        <xs:documentation>(c) Travelport GDS 2008 All rights reserved. Proprietary Information of Travelport GDS.</xs:documentation>
    </xs:annotation>

    <xs:import schemaLocation="../uprofileCommon_v30_0/UprofileCommonReqRsp.xsd" namespace="http://www.travelport.com/schema/uprofileCommon_v30_0"/>

    
   

   <xs:element name="ProfileChildSummary">
      <xs:complexType>
         <xs:attributeGroup ref="attrsProfileID"/>
         <xs:attribute name="Name" type="common:StringLength1to128" use="required">
            <xs:annotation>
               <xs:documentation>The Name of the profile. For profiles of type Agent, concatenate the GivenName and Surname values from the agent table, and return as a single attribute in the response.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="ProvisioningCode" type="common:StringLength1to128" use="optional">
            <xs:annotation>
               <xs:documentation>The provisioning ID/code of the profile, if applicable. </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="Status" type="common:typeProfileEntityStatus" use="required">
                <xs:annotation>
                    <xs:documentation>Profile status (Active, Inactive, Pending, Disabled).</xs:documentation>
                </xs:annotation>
         </xs:attribute>
         <xs:attribute name="Version" type="common:typeVersion" use="required">
            <xs:annotation>
               <xs:documentation>Version of the profile.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="Control" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
               <xs:documentation>Identify if the entity is a control branch or not.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="Description" type="typeDescription"/>
      </xs:complexType>
   </xs:element>
   
    
   
   <!--*** Customization Shared Elements ***-->
   
    <xs:element name="AccountInfo">
        <xs:annotation>
            <xs:documentation>Account specific profile information</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeAccountTypeProfileInfo">
                    <xs:sequence>
                        <xs:element ref="Address" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Account Address</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Account Phone Number</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ElectronicAddress" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Account Electronic Address</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ExternalIdentifier" minOccurs="0" maxOccurs="unbounded">
                           <xs:annotation>
                               <xs:documentation>Account External Identifier</xs:documentation>
                           </xs:annotation>
                       </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Name" type="common:StringLength1to128" use="required">
                        <xs:annotation>
                            <xs:documentation>The name of the account.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="LocalLanguageName" type="common:StringLength1to128">
                        <xs:annotation>
                            <xs:documentation>The name of the account in the user's local language.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
   
    <xs:element name="AccountingReference">
        <xs:annotation>
            <xs:documentation>Used by the Traveler's employer for budgeting, internal billing or other cost accounting purposes.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeKeyElement">
                	<xs:sequence>
                        <xs:element name="PaymentDetailsRef" minOccurs="0" maxOccurs="unbounded" type="common:typeRef">
                           <xs:annotation>
                               <xs:documentation>A code for categorizing a reference for a Traveler’s bookings.  This is often used by the Traveler’s employer for budgeting, internal billing, or other cost accounting purposes. Util:ReferenceDataRetrieveReq, TypeCode AccountingReferenceType</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Type" type="common:StringLength1to128" use="required">
                        <xs:annotation>
                           <xs:documentation>A code for categorizing a reference for a Traveler’s bookings. This is often used by the Traveler’s employer for budgeting, internal billing, or other cost accounting purposes. Util:ReferenceDataRetrieveReq, TypeCode AccountingReferenceType</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Value" type="common:StringLength1to255" use="required">
                        <xs:annotation>
                            <xs:documentation>The number or alphanumeric code for an employer reference.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="AccountID" type="common:typeProfileID">
                        <xs:annotation>
                            <xs:documentation>A specific reference to a particular account profile.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PriorityOrder" type="typePriorityOrder">
                        <xs:annotation>
                            <xs:documentation>Priority order associated with this AccountingReference.</xs:documentation>
                        </xs:annotation>                        
                    </xs:attribute>
                    <xs:attribute name="OwnerID" type="common:typeProfileID" use="optional">
                        <xs:annotation>
                           <xs:documentation>Id of the profile who owns the Traveler's proprietary data.Should be the immediate parent id of the traveler.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Active" type="xs:boolean" default="true"> 
                        <xs:annotation> 
                            <xs:documentation>Denotes whether the Accounting Reference is Active or Not. Default is true</xs:documentation> 
                        </xs:annotation> 
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>  
   
   <xs:element name="Address">
        <xs:annotation>
            <xs:documentation>Profile Address</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeTaggableAddress">
                    <xs:attribute name="Provisioned" type="xs:boolean" default="false">
                        <xs:annotation>
                            <xs:documentation>Indicator to show if this address is used as the provisioned address.  Default is false.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="OwnerID" type="common:typeProfileID" use="optional">
                        <xs:annotation>
                            <xs:documentation>Id of the profile who owns the Traveler's proprietary data.Should be the immediate parent id of the traveler.</xs:documentation>
                       </xs:annotation>
                    </xs:attribute>                      
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="AddressLine" type="common:StringLength1to128">
        <xs:annotation>
            <xs:documentation>Specifies a single line within an address.</xs:documentation>
        </xs:annotation>
    </xs:element>
    
   <xs:element name="AgencyBaseInfo">
      <xs:annotation>
         <xs:documentation>Information relating to Agency.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>Agency Phone Number</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
   </xs:element>

   <xs:element name="BranchBaseInfo">
      <xs:annotation>
         <xs:documentation>Information relating to Branch.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="Address" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>Branch Address</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>Branch Phone Number</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
   </xs:element>

    <xs:element name="HotelPreference">
        <xs:annotation>
            <xs:documentation>Defines a preference for a particular set of criteria for hotels (e.g., dates, rates, chain, room type).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeBasePreference">
                    <xs:attributeGroup ref="attrsBedTypeMisc"/>
                    <xs:attribute name="CheckInStartDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>A valid date in YYYY-MM-DD format.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="CheckInEndDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>A valid date in YYYY-MM-DD format.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="CorporateDiscountNumber">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="25"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="MaxRoomRateAmount" type="common:typeMoney">
                        <xs:annotation>
                            <xs:documentation>See uAPI Profile Help.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="MultiLevelRateCode">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="25"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="PropertyID">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="20"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="RateCode">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="25"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="SmokingRoom" type="xs:boolean" default="false"/>
                    <xs:attributeGroup ref="attrsSpecialReqMisc"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="OtherPreference">
        <xs:annotation>
            <xs:documentation>Defines a preference for a particular set of criteria (e.g. dates, supplier, etc.) that does not fall into the Air, Rail, Vehicle, or Hotel categories. </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeKeyTaggedElement">
                    <xs:attribute name="Purpose" type="typePreferencePurpose">
                        <xs:annotation>
                            <xs:documentation>The purpose of the preference.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PriorityOrder" type="typePriorityOrder">
                        <xs:annotation>
                            <xs:documentation>Priority order associated with this Preference.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="TripApproval" type="xs:boolean" default="false"/>
                    <xs:attribute name="Inclusive" type="xs:boolean" use="optional" default="true">
                        <xs:annotation>
                            <xs:documentation>Indicates whether this preference is exclusive or inclusive (e.g. preference for not having a queen size bed vs preference to HAVE a queen size bed).</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="OtherSupplierType" type="typeOtherPreference">
                        <xs:annotation>
                            <xs:documentation>The type of the Other Preference.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="SupplierName" type="common:StringLength1to128"/>
                    <xs:attribute name="BookingStartDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>A valid date in YYYY-MM-DD format.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="BookingEndDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>A valid date in YYYY-MM-DD format.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="UsageStartDate" type="xs:date"/>
                    <xs:attribute name="UsageEndDate" type="xs:date"/>
                    <xs:attribute name="GeoPoliticalAreaType" type="typeGeoPoliticalAreaType">
                        <xs:annotation>
                            <xs:documentation>The type of the geographical location.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="GeoPoliticalAreaCode" type="typeGeoPoliticalAreaCode">
                        <xs:annotation>
                            <xs:documentation>The location code of the geographical location.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PreferencePaymentMethod" type="typePreferencePaymentMethod">
                        <xs:annotation>
                            <xs:documentation>See uAPI Profile Help and ReferenceDataRetrieveReq, TypeCode PaymentFormatType</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PaymentDetailsRef" type="common:typeRef">
                        <xs:annotation>
                            <xs:documentation>Used to sync up Keys in FormOfPayment element with Preference attribute PaymentDetailsRef by entering same key number.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="MaxCostAmount" type="common:typeMoney">
                        <xs:annotation>
                            <xs:documentation>See uAPI Profile Help.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Currency" type="common:typeCurrency">
                        <xs:annotation>
                            <xs:documentation>ReferencedataRetrieveReq, Type Code Currency</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="GeneralPreference" type="common:StringLength1to255"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="Advisory">
        <xs:annotation>
            <xs:documentation>A categorization of travel documents and other identification, or other warnings that an agency may need to share with agents. Examples include visas requirements, travel permit requirements, passport requirements, etc. May also include government travel or health advisories.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeKeyElement">
                    <xs:attribute name="Type" type="typeAdvisoryType" use="required">
                        <xs:annotation>
                            <xs:documentation>A categorization of travel documents and other identification, or other warnings that an agency may need to share with agents. Examples include visas requirements, travel permit requirements, passport requirements, etc. May also include government travel or health advisories.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="StartDate" type="xs:date" use="required">
                        <xs:annotation>
                            <xs:documentation>The start date of the advisory</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="EndDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>The end date of the advisory</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Summary" type="common:StringLength1to128">
                        <xs:annotation>
                            <xs:documentation>A summary of this Advisory</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Description" type="common:StringLength1to1000"/>
                    <xs:attribute name="PriorityOrder" type="typePriorityOrder">
                        <xs:annotation>
                            <xs:documentation>Priority order associated with this Advisory.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attributeGroup ref="attrsGeoPoliticalArea"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="VehiclePreference">
        <xs:annotation>
            <xs:documentation>Defines a preference for a particular set of criteria for vehicle (e.g., dates, equipment, vendor, car type).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeBasePreference">
                    <xs:attribute name="CorporateID">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="25"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="PickUpStartDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>A valid date in YYYY-MM-DD format.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PickUpEndDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>A valid date in YYYY-MM-DD format.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="RateCode">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="25"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attributeGroup ref="attrsSpecialEquipMisc"/>
                    <xs:attributeGroup ref="attrsSpecialReqMisc"/>
                    <xs:attributeGroup ref="attrsVehicleTypeMisc"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="AgencyInfo">
      <xs:annotation>
         <xs:documentation>Information relating to Agency</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="typeProfileInfo">
                <xs:sequence>
                    <xs:element ref="Advisory" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A set of documents or other advice for travel purposes that an agency recommends that a traveler have, based on a destination."</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="Address" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Agency Address</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Agency Phone Number</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="ElectronicAddress" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Agency Electronic Address</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="ExternalIdentifier" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Agency External Identifier</xs:documentation>
                        </xs:annotation>
                    </xs:element>
               </xs:sequence>
               <xs:attribute name="Name" type="common:StringLength1to128" use="required">
                  <xs:annotation>
                     <xs:documentation>Agency name. Modifying this value requires special authorization.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="IataNumber" type="common:StringLength1to8">
                  <xs:annotation>
                     <xs:documentation>An IATA number associated to the agency. Not used to transact with host systems. Specified in the profile only to support searching for the agency profile by IATA number.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="AgencyCode" type="common:typeProvisioningCode" use="required">
                  <xs:annotation>
                        <xs:documentation>Zircon site ID. Modifying this value requires special authorization.</xs:documentation>
                  </xs:annotation>
              </xs:attribute>
              <xs:attribute name="UsesTemplate" type="xs:boolean" use="optional" default="false">
                 <xs:annotation>
                        <xs:documentation>If set to true, it denotes that the customer uses templates and during parent data inheritance, templates will be used. Value can be altered through modify service.Default is false.</xs:documentation>
                 </xs:annotation>
              </xs:attribute>
              <xs:attributeGroup ref="attrsProfileSync"/>
              <xs:attribute name="URSyncData" use="optional" default="Masked">
                  <xs:annotation>
                     <xs:documentation>If set to 'Masked' then Form Of Payment card number will be masked when Universal Record is sent to EventHandler.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="Masked"/>
                        <xs:enumeration value="Unmasked"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="URSyncTo" type="xs:boolean" use="optional" default="false">
                  <xs:annotation>
                     <xs:documentation>Identify if Universal Record synch is activated at Agency Level. </xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>

   <xs:element name="BranchInfo">
        <xs:annotation>
            <xs:documentation>Information relating to Branch</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeProfileInfo">
                    <xs:sequence>
                        <xs:element ref="Address" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Branch Address.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Branch Phone Number.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ElectronicAddress" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Branch Electronic Address</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ProviderInfo" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>PCCs and IATAs associated to the branch. Note that this profile information is not used to transact with host systems. The data is specified in the profile only to support searching for the branch profile by PCC/IATA. Modifying this data requires special authorization.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ExternalIdentifier" minOccurs="0" maxOccurs="unbounded">
                           <xs:annotation>
                               <xs:documentation>Branch External Identifier</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Name" type="common:StringLength1to128" use="required">
                        <xs:annotation>
                            <xs:documentation>Branch name. Modifying this value requires special authorization.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="GeoCityCode" type="typeGeoCityCode" use="required">
                        <xs:annotation>
                            <xs:documentation>Branch geo city code.  Codes can be found in Ref Pub.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Control" type="xs:boolean" use="required">
                        <xs:annotation>
                            <xs:documentation>Identifies whether the entity is a control branch. Modifying this value requires special authorization.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="BranchCode" type="common:typeProvisioningCode" use="required">
                        <xs:annotation>
                            <xs:documentation>Zircon site ID. Modifying this value requires special authorization.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Currency" type="common:typeCurrency" use="required">
                        <xs:annotation>
                            <xs:documentation>Default currency specified for the branch.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attributeGroup ref="attrsProfileSync"/>
                    <xs:attribute name="URSyncTo" type="xs:boolean" use="optional" default="false">
                        <xs:annotation>
                            <xs:documentation>Identify if Universal Record synch is activated at Branch Level.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
     
   <xs:element name="AgencyGroupInfo">
        <xs:annotation>
            <xs:documentation>Information relating to Agency Group</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeProfileInfo">
                    <xs:sequence>
                        <xs:element ref="Address" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Agency Group Address</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Agency Group Phone Number</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ElectronicAddress" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Agency Group Electronic Address</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ExternalIdentifier" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Agency Group External Identifier</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Name" type="common:StringLength1to128" use="required">
                        <xs:annotation>
                            <xs:documentation>Name of Agency Group</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="AgentInfo">
      <xs:annotation>
         <xs:documentation>Agent specific profile information</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="typeProfileInfo">
                <xs:sequence>
                    <xs:element ref="Address" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Agent Address</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Agent Phone Number</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="ElectronicAddress" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Agent Electronic Address.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="ExternalIdentifier" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Agent External Identifier</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
               <xs:attribute name="UserName" type="common:StringLength1to128" use="required">
                  <xs:annotation>
                     <xs:documentation>The login name of the agent. Modifying this value requires special authorization.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="OccupationalTitle" type="common:StringLength1to255" use="optional"/>
               <xs:attribute name="Title" type="common:StringLength1to128" use="optional"/>
               <xs:attribute name="Nickname" type="common:StringLength1to128" use="optional"/>
               <xs:attribute name="GivenName" type="common:StringLength1to128" use="required">
                  <xs:annotation>
                     <xs:documentation>Given name of the agent. Modifying this value requires special authorization.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="OtherName" type="common:StringLength1to128" use="optional"/>
               <xs:attribute name="Surname" type="common:StringLength1to128" use="required">
                  <xs:annotation>
                     <xs:documentation>Surname of the agent. Modifying this value requires special authorization.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Suffix" type="common:StringLength1to128" use="optional"/>
               <xs:attribute name="DefaultBranchID" type="common:typeProfileID">
                    <xs:annotation>
                        <xs:documentation>The profile ID of the branch assigned as the default branch for this agent. If both the profile ID and branch code of the default branch are specified, the ID will be used.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="DefaultBranchCode" type="common:typeProvisioningCode">
                    <xs:annotation>
                        <xs:documentation>The branch code of the branch assigned as the default branch for this agent. If both the profile ID and branch code of the default branch are specified, the ID will be used.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="AlternateAgentID" type="typeAlternateAgentID" use="optional"/>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="BaseInfo">
      <xs:complexType>
         <xs:choice>
            <xs:element ref="AgencyBaseInfo" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Information relating to Agency.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="BranchBaseInfo" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Information relating to Branch.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="BranchGroupInfo">
        <xs:annotation>
            <xs:documentation>Branch group specific profile information</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeProfileInfo">
                    <xs:sequence>
                        <xs:element ref="Address" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Branch Group Address</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Branch Group  Phone Number</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ElectronicAddress" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Branch Group Electronic Address</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ExternalIdentifier" minOccurs="0" maxOccurs="unbounded">
                           <xs:annotation>
                               <xs:documentation>Branch Group External Identifier</xs:documentation>
                           </xs:annotation>
                       </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Name" type="common:StringLength1to128" use="required">
                        <xs:annotation>
                            <xs:documentation>Branch group name. Modifying this value requires special authorization.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="BranchGroupCode" type="common:typeProvisioningCode" use="required">
                        <xs:annotation>
                            <xs:documentation>Zircon site ID. Modifying this value requires special authorization.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attributeGroup ref="attrsProfileSync"/>
                    <xs:attribute name="URSyncTo" type="xs:boolean" use="optional" default="false">
                        <xs:annotation>
                            <xs:documentation>Identify if Universal Record synch is activated at Branch Group Level.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
   </xs:element>
   
   <xs:element name="Commission">
        <xs:annotation>
            <xs:documentation>A representation of the commision given to an Agent or Agency within a profile.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeKeyElement">
                    <xs:attribute name="Type" type="typeCommissionCategory" use="required">
                        <xs:annotation>
                            <xs:documentation>A type that represents a category of commissions.  Example include per booking, by monetary amount, etc.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="SupplierType" type="typeSupplierType" use="required">
                        <xs:annotation>
                            <xs:documentation>The commission's supplier type</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Supplier" type="typeSupplier" use="required">
                        <xs:annotation>
                            <xs:documentation>The commission's supplier</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Amount" type="common:typeMoney">
                        <xs:annotation>
                            <xs:documentation>A financial amount for a specific commission type for a travel supplier.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Percentage" type="common:typePercentageWithDecimal">
                        <xs:annotation>
                            <xs:documentation>A percentage for a specific commission type for a travel supplier.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PriorityOrder" type="typePriorityOrder">
                        <xs:annotation>
                            <xs:documentation>Priority order associated with this Commission.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="ProprietaryData">
       <xs:annotation>
          <xs:documentation>ProprietaryData for a Traveler which can be overridden for immediate parent like BranchGroup,Branch,Account and TravelerGroup.</xs:documentation>
        </xs:annotation>  
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeKeyElement">
                    <xs:attribute name="ProprietaryDataType" use="required">
                         <xs:annotation>
                            <xs:documentation>The type of ProprietaryData being overridden for a TravelerProfile.</xs:documentation>
                          </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="NickName"/>
                                <xs:enumeration value="Description"/>
                                <xs:enumeration value="AdditionalIdentifier"/>
                                <xs:enumeration value="ExternalIdentifier"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="Value" type="common:StringLength1to255" use="required">
                         <xs:annotation>
                            <xs:documentation>The value of ProprietaryData.</xs:documentation>
                         </xs:annotation>                   
                    </xs:attribute>
                    <xs:attribute name="OwnerID" type="common:typeProfileID" use="required">
                        <xs:annotation>
                           <xs:documentation>Id of the profile who owns the Traveler's proprietary data.Should be the immediate parent id of the traveler.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    
   <xs:element name="CommissionReference">
      <xs:annotation>
         <xs:documentation>Data refering to an external or internal Agent who receives commission with relation to this profile. Agent Name or Number is required.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="typeKeyElement">
               <xs:attribute name="AgentName" type="common:StringLength1to128">
                  <xs:annotation>
                     <xs:documentation>Name of the agent.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="AgentNumber" type="common:StringLength1to128">
                  <xs:annotation>
                     <xs:documentation>Number or code identifying the agent.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PriorityOrder" type="typePriorityOrder">
                  <xs:annotation>
                     <xs:documentation>Priority of this commission reference item.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="OwnerID" type="common:typeProfileID" use="optional">
                   <xs:annotation>
                      <xs:documentation>Id of the profile who owns the Traveler's proprietary data.Should be the immediate parent id of the traveler.</xs:documentation>
                   </xs:annotation>
               </xs:attribute>               
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
    
    <xs:element name="Contract">
        <xs:annotation>
            <xs:documentation>A representation of a contract given between the
                agency and a supplier/provider.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeKeyTaggedElement">
                    <xs:attribute name="Supplier" type="typeSupplier" use="required">
                        <xs:annotation>
                            <xs:documentation>The supplier associated with the contract.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="SupplierType" type="typeSupplierType"
                        use="required">
                        <xs:annotation>
                            <xs:documentation>The supplier type associated with the contract.
                                (Air, Hotel, etc)</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Provider" type="common:typeProviderCode"
                        use="required">
                        <xs:annotation>
                            <xs:documentation>The provider associated with the contract.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="StartDate" type="xs:date" use="required">
                        <xs:annotation>
                            <xs:documentation>The date that contract terms start to be
                                applied.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ExpirationDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>The date that contract terms terminate being
                                applied.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="DiscountPercentage" type="common:typePercentageWithDecimal">
                        <xs:annotation>
                            <xs:documentation>The percentage of discount applicable to a
                                contract.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="DiscountValue" type="common:typeMoney">
                        <xs:annotation>
                            <xs:documentation>The amount of discount applicable to a
                                contract.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="SupplierContractNumber" type="common:StringLength1to50" use="required">
                        <xs:annotation>
                            <xs:documentation>An eternally assigned or managed number or alphanumeric data.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PromotionalDesignatorName" type="common:StringLength1to255">
                        <xs:annotation>
                            <xs:documentation>An eternally assigned or managed number or
                                alphanumeric data for a ticket designator.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Description" type="common:StringLength1to255">
                        <xs:annotation>
                            <xs:documentation>Description of the contract.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PriorityOrder" type="typePriorityOrder">
                        <xs:annotation>
                            <xs:documentation>Priority order associated with this Contract.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
   <xs:element name="CreateField">
      <xs:annotation>
         <xs:documentation>Specify any existing fields that belong to this group.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:choice>
               <xs:element name="FreeformTextRestriction" type="typeStringRestriction" minOccurs="0">
                  <xs:annotation>
                     <xs:documentation>Restrictions on profile data for fields with a data type of free-form text (i.e., string). Min and max lengths are inclusive. Can define either min/max lengths or enumerations, but not both.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="WholeNumberRestriction" type="typeIntegerRestriction" minOccurs="0">
                  <xs:annotation>
                     <xs:documentation>Restrictions on profile data for fields with a data type of integer. Min and max values are inclusive.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="DecimalRestriction" type="typeFloatRestriction" minOccurs="0">
                  <xs:annotation>
                     <xs:documentation>Restrictions on profile data for fields with a data type of float. Min and max values are inclusive.</xs:documentation>
                  </xs:annotation>
               </xs:element>
                <xs:element name="TextRestriction" type="typeStringRestriction" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Restrictions on profile data for fields with a data type of text (i.e., alpha). Min and max lengths are inclusive. Can define either min/max lengths or enumerations, but not both.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="AlphaNumericRestriction" type="typeStringRestriction" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Restrictions on profile data for fields with a data type of alphanumeric. Min and max lengths are inclusive. Can define either min/max lengths or enumerations, but not both.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="PercentageRestriction" type="typeFloatRestriction" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Restrictions on profile data for fields with a data type of percentage. Min and max values are inclusive.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
         </xs:sequence>
         <xs:attributeGroup ref="attrsField"/>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="CreateFieldGroup">
        <xs:annotation>
            <xs:documentation>Defines the structure of a new field group, which can be based on existing fields and groups (referred to by Id) and/or new fields and groups (referred to by FieldGroupRef or FieldRef and defined in FieldList or FieldGroupList).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="CreateField" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Specify fields that belong to this group.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attributeGroup ref="attrsFieldGroup"/>
        </xs:complexType>
    </xs:element>
   
   <xs:element name="Field"> 
        <xs:annotation> 
            <xs:documentation>Specify any existing fields that belong to this group.</xs:documentation> 
        </xs:annotation> 
        <xs:complexType> 
            <xs:complexContent> 
                <xs:extension base="typeCustomField"> 
                    <xs:attribute name="ProfileID" type="common:typeProfileID" use="required"> 
                        <xs:annotation> 
                            <xs:documentation>The profile ID of the agency or account that the field belongs to.</xs:documentation> 
                        </xs:annotation> 
                    </xs:attribute>
                    <xs:attribute name="ProfileType" type="typeProfileType" use="required"> 
                        <xs:annotation> 
                            <xs:documentation>The type of the Profile to be created.</xs:documentation> 
                        </xs:annotation> 
                    </xs:attribute>
                    <xs:attribute name="IsUsed" type="xs:boolean" use="required">
                        <xs:annotation>
                            <xs:documentation>True if the custom field is in use by one or more profiles.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
   
   <xs:element name="FieldData">
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="typeKeyTaggedElement">
               <xs:attribute name="Value" type="common:StringLength1to255" use="required">
                  <xs:annotation>
                     <xs:documentation>The value of this instance of the field.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="FieldID" type="typeFieldID" use="optional">
                  <xs:annotation>
                     <xs:documentation>The unique ID of the template field (instance of a field on a template) that this value is applied to.Provide either FieldID or Name, not both.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Name" type="common:StringLength1to255" use="optional">
                  <xs:annotation>
                     <xs:documentation>Name of the custom field that this value is applied to.Provide either Name or FieldID, not both.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="OwnerID" type="common:typeProfileID" use="optional">
                  <xs:annotation>
                     <xs:documentation>Id of the profile who owns the Traveler's proprietary data.Should be the immediate parent id of the traveler.</xs:documentation>
                  </xs:annotation>
              </xs:attribute>               
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="FieldDataSearch">
        <xs:annotation>
            <xs:documentation>Specifies a search term and value (wildcards permitted), to search profiles by data in custom profile fields.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="FieldID" type="typeFieldID" use="optional">
                <xs:annotation>
                    <xs:documentation>The reference to the custom field that this FieldData refers.Provide either FieldID or Name, not both.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
             <xs:attribute name="Name" type="common:StringLength1to255" use="optional">
                <xs:annotation>
                    <xs:documentation>Name of the custom field that this FieldData refers.Provide either Name or FieldID, not both.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Value" use="required">
                <xs:annotation>
                    <xs:documentation>The value to search for in the relevant profiles. Wildcards permitted. All searches are case-insensitive. </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="common:typeWildcard">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="FieldGroupID" type="typeFieldID">
                <xs:annotation>
                    <xs:documentation>Used to limit the search to multiple fields of the same field group instance. </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="FieldGroup">
        <xs:annotation>
            <xs:documentation>Defines the structure of a new field group, which can be based on existing fields and groups (referred to by Id) and/or new fields and groups (referred to by FieldGroupRef or FieldRef and defined in FieldList or FieldGroupList).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Field" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Specify fields that belong to this group.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="ID" type="typeID" use="required">
                <xs:annotation>
                    <xs:documentation>Unique identifier of the field group.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attributeGroup ref="attrsFieldGroup"/>
            <xs:attribute name="ProfileID" type="common:typeProfileID" use="required"> 
                <xs:annotation> 
                    <xs:documentation>The profile ID of the agency or account that the field belongs to.</xs:documentation> 
                </xs:annotation> 
            </xs:attribute>
            <xs:attribute name="ProfileType" type="typeProfileType" use="required"> 
                <xs:annotation> 
                    <xs:documentation>The type of the Profile to be created.</xs:documentation> 
                </xs:annotation> 
            </xs:attribute>
            <xs:attribute name="IsUsed" type="xs:boolean" use="required">
                <xs:annotation>
                    <xs:documentation>True if the custom field group has been added to a template.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="FieldGroupData">
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="typeKeyTaggedElement">
               <xs:sequence>
                  <xs:element ref="FieldData" minOccurs="0" maxOccurs="unbounded">
                     <xs:annotation>
                        <xs:documentation>Data values associated to this entity. Each value is linked to a field instance, which defines the attributes of the field specific to the associated template or its parent field group.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="FieldGroupID" type="typeFieldID" use="required">
                  <xs:annotation>
                     <xs:documentation>The unique ID of the template field group (instance of a field group on a template) that this value is applied to.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="OwnerID" type="common:typeProfileID" use="optional">
                  <xs:annotation>
                     <xs:documentation>Id of the profile who owns the Traveler's proprietary data.Should be the immediate parent id of the traveler.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>                 
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="LoyaltyProgramEnrollment">
        <xs:annotation>
            <xs:documentation>Used for maintaining data for loyalty programs between a supplier and a traveler.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeKeyTaggedElement">
                    <xs:attribute name="SupplierType" type="typeSupplierType" use="required">
                        <xs:annotation>
                            <xs:documentation>The commission's supplier type</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Supplier" type="typeSupplier" use="required">
                        <xs:annotation>
                            <xs:documentation>The commission's supplier</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Number" type="common:StringLength1to255" use="required">
                        <xs:annotation>
                            <xs:documentation>A traveler's specific loyalty program number or alphanumeric identifier.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ProgramName" type="common:StringLength1to128">
                        <xs:annotation>
                            <xs:documentation>Supplier's loyalty program name such as Frontier-EarlyReturns, Renaissance Intl-Marriott Rewards,Alamo-QuickSilver Service etc.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Status" type="common:StringLength1to128">
                        <xs:annotation>
                            <xs:documentation>Categorize a status within a loyalty program.  Examples include Super Elite, Elite, Premier, Prestige, Platinum, regular, etc.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PriorityOrder" type="typePriorityOrder">
                        <xs:annotation>
                            <xs:documentation>Priority order associated with this LoyaltyProgramEnrollment.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="ModifyField">
      <xs:annotation>
         <xs:documentation>Details of a root-level field to be updated. Root-level fields cannot be created or deleted with this service. Child fields must be modified within ModifyFieldGroup. When updating a field, to remove data from an optional attribute, omit the attribute. Note that Display Order is not applicable to root-level fields.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:annotation>
               <xs:documentation>Several restrictions can be specified to support a change in type (e.g. changing from String to Float).</xs:documentation>
            </xs:annotation>
            <xs:element name="FreeformTextRestriction" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Restrictions on profile data for fields with a data type of free-form text (i.e., string). Min and max lengths are inclusive. Can define either min/max lengths or enumerations, but not both.</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:complexContent>
                     <xs:extension base="typeStringRestriction">
                        <xs:attribute name="Action" type="typeAction" use="required">
                           <xs:annotation>
                              <xs:documentation>Indicate the action to be executed (update or delete)</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:extension>
                  </xs:complexContent>
               </xs:complexType>
            </xs:element>
            <xs:element name="WholeNumberRestriction" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Restrictions on profile data for fields with a data type of integer. Min and max values are inclusive.</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:complexContent>
                     <xs:extension base="typeIntegerRestriction">
                        <xs:attribute name="Action" type="typeAction" use="required">
                           <xs:annotation>
                              <xs:documentation>Indicate the action to be executed (update or delete)</xs:documentation>
