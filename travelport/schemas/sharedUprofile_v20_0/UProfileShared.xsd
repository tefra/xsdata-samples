            <xs:attribute name="Version" type="common:typeVersion" use="required">
                <xs:annotation>
                    <xs:documentation>Version number of the profile.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    
        <xs:element name="ModifyTag" >
             <xs:annotation>
                <xs:documentation/>
             </xs:annotation>
