            </xs:sequence>
        </xs:complexType>
    </xs:element>
        
    <xs:element name="ProfileSearchModifiers">
        <xs:annotation>
            <xs:documentation>Specifies the range of search results. If omitted, the default values for each attriubute are used.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="MaxResults" type="common:typeMaxResults" default="20">
                <xs:annotation>
                    <xs:documentation>Limits the number of search results in the response. Note that performance may decline when a large number of results is requested.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="StartFromResult" type="common:typeStartFromResult" default="1">
                <xs:annotation>
                    <xs:documentation>The starting search index of the results returned. Used to browse beyond the initial search results.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="ProfileStatusUpdate">
        <xs:annotation>
            <xs:documentation>Change the status of the profile.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Status" type="common:typeProfileEntityStatus" use="required">
                <xs:annotation>
                    <xs:documentation>The updated status. Inactive status is not valid for all profile types.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="ProfileLinkAdd">
        <xs:annotation>
            <xs:documentation>Add a new link between this profile and the specific profile.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attributeGroup ref="attrsProfileLink"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="ProfileLinkDelete">
        <xs:annotation>
