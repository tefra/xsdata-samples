                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="ExternalIdentifier" minOccurs="0" maxOccurs="unbounded">
                           <xs:annotation>
                               <xs:documentation>Traveler Group External Identifier</xs:documentation>
                           </xs:annotation>
                       </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Name" type="common:StringLength1to128" use="required">
