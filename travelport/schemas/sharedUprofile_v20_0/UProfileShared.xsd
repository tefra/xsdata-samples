            <xs:element ref="AlternateContact" maxOccurs="unbounded"/>
            <xs:element ref="AlternateContactAddress" maxOccurs="unbounded"/>
            <xs:element ref="AlternateContactPhone" maxOccurs="unbounded"/>
            <xs:element ref="AlternateContactElectronicAddress" maxOccurs="unbounded"/>
            <xs:element ref="FormOfPayment"  minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="AirPreference" maxOccurs="unbounded"/>
            <xs:element ref="HotelPreference" maxOccurs="unbounded"/>
            <xs:element ref="OtherPreference" maxOccurs="unbounded"/>
            <xs:element ref="Remark" maxOccurs="unbounded"/>
            <xs:element ref="FieldData" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Change the values of existing root-level or child-level custom fields. To add a new child field value, use FieldData within FieldGroupData. To delete an existing child field value, use ProfileDataDelete.</xs:documentation>
                    </xs:annotation>
            </xs:element>
            <xs:element ref="FieldGroupData" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Add new child field values or modify existing child field values within an existing custom field group instance. To delete a field group instance and its child field values, use ProfileDataDelete.</xs:documentation>
                    </xs:annotation>
            </xs:element>
            <xs:element ref="VehiclePreference" maxOccurs="unbounded"/>
            <xs:element ref="Advisory" maxOccurs="unbounded"/>
            <xs:element ref="CommissionReference" maxOccurs="unbounded"/>
            <xs:element ref="RailPreference" maxOccurs="unbounded"/>
            <xs:element ref="ProprietaryData" maxOccurs="unbounded"/>            
         </xs:choice>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="ProfileDataDelete">
      <xs:annotation>
         <xs:documentation>Delete one element of given type identified by its key.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:attribute name="Element" type="typeProfileDataElementType" use="required">
            <xs:annotation>
               <xs:documentation>The type of element in the profile.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="Key" type="common:typeRef" use="required">
            <xs:annotation>
               <xs:documentation>The identifier of the element that will be delete from this profile.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="ProfileModifyCmd">
      <xs:annotation>
         <xs:documentation>Wrapper for a set of modification commands to be applied to this profile.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:choice>
            <xs:element ref="ProfileStatusUpdate"/>
            <xs:element ref="ProfileLinkAdd">
               <xs:annotation>
                  <xs:documentation>Add a new link between this profile and the specific profile.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="ProfileLinkDelete">
               <xs:annotation>
                  <xs:documentation>Delete the link between this profile and the specificied profile.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="ProfileParentAdd">
                <xs:annotation>
                    <xs:documentation>Command to add a new parent profile.  Either ProvisioningCode or ProfileID are required.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="ProfileParentDelete">
                <xs:annotation>
                    <xs:documentation>Command to delete a parent profile.  Either ProvisioningCode or ProfileID are required.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="ProfileDataAdd"/>
            <xs:element ref="ProfileDataUpdate"/>
            <xs:element ref="ProfileDataDelete"/>
            <xs:element ref="TagAdd"/>
            <xs:element ref="TagDelete"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="HistoryElement">
        <xs:annotation>
            <xs:documentation>The history of a particular element at a particular point in time.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Original" type="typeHistorySubElement" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The orignal values of the element before the change.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="New" type="typeHistorySubElement" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The new updated values of the element.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="Action" type="typeAction" use="required">
                <xs:annotation>
                    <xs:documentation>The action made on the element.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ModifiedByAgentID" type="common:typeProfileID" use="required">
                <xs:annotation>
                    <xs:documentation>Agent who modified this element</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ModifiedByAgentUserName" type="common:typeAgentCode" use="required">
                <xs:annotation>
                    <xs:documentation>Agent who last modified the profile</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ModifiedDate" type="xs:dateTime" use="required">
                <xs:annotation>
                    <xs:documentation>When this element was modified</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Component" use="required">
                <xs:annotation>
                    <xs:documentation>The corresponding name of the element that this field refers to in the profile message. This is a read only attribute.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="CorrelationElement" use="optional">
                <xs:annotation>
                    <xs:documentation>The name of an attribute on the profile element that defines a "type".  This is a read-only attribute.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="CorrelationValue" type="common:StringLength1to128" use="optional">
                <xs:annotation>
                    <xs:documentation>The value that should be put in the attribute that the CorrelationElement defines.  This is a read-only attribute.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>



    <xs:element name="ProfileHistory">
        <xs:annotation>
            <xs:documentation>The profile history</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="HistoryElement" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attributeGroup ref="attrsProfileID"/>
            <xs:attribute name="ProfileName" use="required">
                <xs:annotation>
                    <xs:documentation>The name of the profile. Either the the organization name, or the concatenated first and last names of a profile representing a person (agent or traveler).</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CreatedByAgentID" type="common:typeProfileID" use="optional">
                <xs:annotation>
                    <xs:documentation>Agent who created the Profile</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CreatedByAgentUserName" type="common:typeAgentCode" use="optional">
                <xs:annotation>
                    <xs:documentation>Agent who created the Profile</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CreatedDate" type="xs:dateTime" use="optional">
                <xs:annotation>
                    <xs:documentation>Date that the profile was created</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LastModifiedByAgentID" type="common:typeProfileID">
                <xs:annotation>
                    <xs:documentation>Agent who last modified the profile</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LastModifiedByAgentUserName" type="common:typeAgentCode">
                <xs:annotation>
                    <xs:documentation>Agent who last modified the profile</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LastModifiedDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>The date that the profile was last modified</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="ProfileHistoryRetrieveCriteria">
        <xs:annotation>
            <xs:documentation>Specify one or more criteria used to filter the history data. Only history data matching all parameters will be returned.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="AgentID" type="common:typeProfileID" use="optional">
                <xs:annotation>
                    <xs:documentation>The unique profile ID of the agent that made the change.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="StartDate" type="xs:date" use="optional">
                <xs:annotation>
                    <xs:documentation>The earliest date that the change was made. Leave blank to define an open start date.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="EndDate" type="xs:date" use="optional">
                <xs:annotation>
                    <xs:documentation>The latest date that the change was made. Leave blank to define an open end date.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FieldID" type="typeFieldID" use="optional">
                <xs:annotation>
                    <xs:documentation>To filter by a specific custom field, enter the field ID. To filter by a specific instance of a fixed field or field group, send the field or field group Key. Must also send the relevant FieldGroupType value.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FieldGroupID" type="typeFieldID" use="optional">
                <xs:annotation>
                    <xs:documentation>To filter by a specific custom field group, enter the field group ID. Must also send the relevant FieldGroupType value.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FieldGroupType" type="typeProfileComponentType" use="optional">
                <xs:annotation>
                    <xs:documentation>To filter by a type of fixed field group or to filter for all custom field data or all custom field group data, send the appropriate field group type. To further refine the search by fixed field or group instance or by custom field or field group ID, also send FieldID or FieldGroupID.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    
   <xs:element name="ProfileParent" type="typeProfileParentWithData">
      <xs:annotation>
         <xs:documentation>A parent's profile, including the profile data (if specified in the request) and a list of its parents as well.</xs:documentation>
      </xs:annotation>
   </xs:element>
    
    <xs:element name="ProfileParentAdd">
