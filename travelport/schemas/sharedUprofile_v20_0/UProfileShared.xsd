                            <xs:documentation>The supplier code of the payment. (VI, CA, AX, etc)</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="AccountNumber" type="common:StringLength1to128" use="required">
                        <xs:annotation>
                            <xs:documentation>Payment account number. (ie. Credit card number, etc)</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Description" type="common:StringLength1to255">
