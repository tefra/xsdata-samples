    <xs:element name="ProfileSearch">
        <xs:annotation>
            <xs:documentation>All the fixed fields allowed for searching</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="ContractNumber" type="common:typeWildcardMax50" minOccurs="0"/>
                <xs:element name="TravelDocumentNumber" type="common:StringLength1to128" minOccurs="0"/>
                <xs:element name="AccountingReference" type="typeSearchAccountingReference" minOccurs="0"/>
                <xs:element name="AlternateContact" type="typeSearchContact" minOccurs="0"/>
                <xs:element name="LoyaltyProgram" type="typeSearchLoyaltyProgram" minOccurs="0"/>
                <xs:element name="PaymentDetails" type="typeSearchPaymentDetails" minOccurs="0"/>
