     <xs:complexType name="typeFormOfPaymentType">
        <xs:complexContent>
            <xs:extension base="typeKeyTaggedElement">
                <xs:choice>
                    <xs:element ref="CreditCard" minOccurs="0"/>
                    <xs:element name="DebitCard" type="common:typePaymentCard" minOccurs="0"/>
                    <xs:group ref="common:FormOfPaymentSequenceGroup"/>
                    <xs:element name="Guarantee" type="common:typeGuaranteeInformation" minOccurs="0"/>
                    <xs:element name="Voucher" type="common:typeVoucherInformation" minOccurs="0"/>
                    <xs:element name="Cash" type="common:typeMoney" minOccurs="0"/>
                </xs:choice>
                <xs:attribute name="Type">
                 <xs:annotation>
                    <xs:documentation>The code from the PAYMENT_FORMAT_TYPE_CD (CRECRD, DEBDRC, REQSTN…).</xs:documentation>
                 </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Description" type="common:StringLength1to255">
                    <xs:annotation>
                        <xs:documentation>Description of the Payment</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="PriorityOrder" type="typePriorityOrder">
                    <xs:annotation>
                        <xs:documentation>Priority order associated with this PaymentDetails.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="OwnerID" type="common:typeProfileID" use="optional">
                    <xs:annotation>
                        <xs:documentation>Id of the profile who owns the Traveler's proprietary data.Should be the immediate parent id of the traveler.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>  
    
    <xs:element name="FormOfPayment" type="typeFormOfPaymentType"/>
    
    <xs:complexType name="typeFormOfPaymentHistory">
                <xs:complexContent>
                        <xs:extension base="typeKeyElement">
                                <xs:choice>
                                        <xs:element name="CreditCard" type="typeCreditCardTypeHistory" minOccurs="0"/>
                                        <xs:element name="DebitCard" type="typePaymentCardHistory" minOccurs="0"/>
                                        <xs:group ref="FormOfPaymentSequenceGroupHistory"  minOccurs="0"/>
                                        <xs:element name="Guarantee" type="typeGuaranteeInformationHistory" minOccurs="0"/>

                                        <xs:element name="Voucher" type="typeVoucherInformationHistory" minOccurs="0"/>
                                        <xs:element name="Cash" type="common:typeMoney" minOccurs="0"/>
                                </xs:choice>
                                <xs:attribute name="Type" use="optional">
                                 <xs:annotation>
                                <xs:documentation>The code from the PAYMENT_FORMAT_TYPE_CD (CRECRD, DEBDRC, REQSTN…).</xs:documentation>

                         </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="Description" type="common:StringLength1to255" use="optional">
                                        <xs:annotation>
                                                <xs:documentation>Description of the Payment</xs:documentation>
                                        </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="PriorityOrder" type="typePriorityOrder" use="optional">
                                        <xs:annotation>
                                                <xs:documentation>Priority order associated with this PaymentDetails.</xs:documentation>

                                        </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="OwnerID" type="common:typeProfileID" use="optional">
                                        <xs:annotation>
                                                <xs:documentation>Id of the profile who owns the Traveler's proprietary data.Should be the immediate parent id of the traveler.</xs:documentation>

                                        </xs:annotation>
                                </xs:attribute>
                        </xs:extension>
                </xs:complexContent>
        </xs:complexType>
       
        <xs:complexType name="typeCreditCardTypeHistory">
                <xs:complexContent>
                        <xs:extension base="typePaymentCardHistory">
                                <xs:attribute name="ExtendedPayment" type="xs:string" use="optional">
                                        <xs:annotation>
                                                <xs:documentation>Used for American Express cards.
                            </xs:documentation>
                                        </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="CustomerReference" type="xs:string" use="optional">
                                        <xs:annotation>
                                                <xs:documentation>Agencies use this to pass the traveler information to the credit card company.</xs:documentation>

                                        </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="AcceptanceOverride" type="xs:boolean" use="optional">
                                        <xs:annotation>
                                                <xs:documentation>Override airline restriction on the credit card.</xs:documentation>

                                        </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="ThirdPartyPayment" type="xs:boolean" use="optional" default="false">

                                        <xs:annotation>
                                                <xs:documentation>If true, this indicates that the credit card holder is not one of the passengers.</xs:documentation>

                                        </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="BankName" type="xs:string" use="optional">
                                        <xs:annotation>
                                                <xs:documentation>Issuing bank name for this credit card</xs:documentation>

                                        </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="BankCountryCode" type="common:typeCountry" use="optional">
                                        <xs:annotation>
                                                <xs:documentation>ISO Country code associated with the issuing bank</xs:documentation>

                                        </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="ExtractIndicator" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                        <xs:documentation>This is supported for all Profile Types by Universal Profile.This indicator will be used by MOS to create a Credit Card Extract.</xs:documentation>
                                    </xs:annotation>
                               </xs:attribute>
                               <xs:attribute name="Active"  type="xs:boolean" use="optional">
                                    <xs:annotation>
                                        <xs:documentation>Denotes whether the Credit Card is Active or Not.</xs:documentation>
                                    </xs:annotation>
                               </xs:attribute>
                        </xs:extension>
                </xs:complexContent>
        </xs:complexType>
       
        <xs:complexType name="typePaymentCardHistory">
        <xs:annotation>
            <xs:documentation>Container for all credit and debit card
                information.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="PhoneNumberHistory" minOccurs="0" />
            <xs:element name="BillingAddress" type="common:typeStructuredAddress"
                minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The address to where the billing statements
                        for this card are sent. Used for address verification purposes.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Type" use="optional" type="common:typeCardMerchantType">
            <xs:annotation>
                <xs:documentation>The 2 letter credit/ debit card type.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Number" type="common:typeCreditCardNumber" use="optional"/>
        <xs:attribute name="ExpDate" type="xs:gYearMonth" use="optional">
            <xs:annotation>
                <xs:documentation>The Expiration date of this card in YYYY-MM format.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Name" use="optional">
            <xs:annotation>
                <xs:documentation>The name as it appears on the card.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="128" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="CVV" use="optional">
            <xs:annotation>
                <xs:documentation>Card Verification Code</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="4" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="ApprovalCode" use="optional">
            <xs:annotation>
                <xs:documentation>This code is optional for an authorization process from the Credit Card company directly,optional for some of the CCH carriers.</xs:documentation>

            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="16" />
                    <xs:minLength value="1" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
   
    <xs:element name="PhoneNumberHistory">
        <xs:annotation>
            <xs:documentation>Consists of type (office, home, fax), location (city code), the country code, the number, and an extension.</xs:documentation>

        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                    <xs:annotation>
                       <xs:documentation>Tagging provider reservation info with PhoneNumber.</xs:documentation>
                    </xs:annotation>
                    <xs:element ref="common:ProviderReservationInfoRef" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="Key" type="common:typeRef" use="optional"/>
            <xs:attribute name="Type" use="optional">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Agency"/>
                        <xs:enumeration value="Business"/>
                        <xs:enumeration value="Mobile"/>
                        <xs:enumeration value="Home"/>
                        <xs:enumeration value="Fax"/>
                        <xs:enumeration value="Hotel"/>
                        <xs:enumeration value="Other"/>
                        <xs:enumeration value="None"/>
                        <xs:enumeration value="Email"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="Location" use="optional">
                <xs:annotation>
                    <xs:documentation>IATA code for airport or city</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="CountryCode" use="optional">
                <xs:annotation>
                    <xs:documentation>Hosts/providers will expect this to be international dialing digits</xs:documentation>

                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="5"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="AreaCode" use="optional">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="Number" use="optional">
                <xs:annotation>
                    <xs:documentation>The local phone number</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="83"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="Extension" use="optional">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="Text" use="optional">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="1024"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
   
    <xs:group name="FormOfPaymentSequenceGroupHistory">
                <xs:choice>
                        <xs:element ref="CertificateHistory" minOccurs="0"/>
                        <xs:element ref="TicketNumberHistory" minOccurs="0"/>
                        <xs:element ref="common:Check" minOccurs="0"/>
                        <xs:element ref="common:Requisition" minOccurs="0"/>
                        <xs:element ref="MiscFormOfPaymentHistory" minOccurs="0"/>
                        <xs:element ref="AgencyPaymentHistory" minOccurs="0"/>
                        <xs:element ref="UnitedNationsHistory" minOccurs="0"/>
                        <xs:element ref="common:DirectPayment" minOccurs="0"/>
                        <xs:element ref="AgentVoucherHistory" minOccurs="0"/>
                </xs:choice>
        </xs:group>
        
        <xs:element name="TicketNumberHistory" type="StringLength0to13">
         <xs:annotation>
            <xs:documentation>The identifying number for the actual ticket</xs:documentation>
          </xs:annotation>
        </xs:element>
        
        <xs:simpleType name="StringLength0to13">
         <xs:annotation>
         <xs:documentation xml:lang="en">Used for Character Strings, length 0 to 13.</xs:documentation>
        </xs:annotation>
         <xs:restriction base="xs:string">
         <xs:minLength value="0"/>
         <xs:maxLength value="13"/>
         </xs:restriction>
     </xs:simpleType>
       
        <xs:element name="CertificateHistory">
        <xs:annotation>
            <xs:documentation>Certificate Form of Payment</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Number" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>The Certificate number</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Amount" type="common:typeMoney" use="optional">
                <xs:annotation>
                    <xs:documentation>The monetary value of the certificate.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DiscountAmount" type="common:typeMoney" use="optional">
                <xs:annotation>
                    <xs:documentation>The monetary discount amount of this certificate.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DiscountPercentage" type="xs:integer" use="optional">
                <xs:annotation>
                    <xs:documentation>The percentage discount value of this certificate.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="NotValidBefore" type="xs:date" use="optional">
                <xs:annotation>
                    <xs:documentation>The date that this certificate becomes valid.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="NotValidAfter" type="xs:date" use="optional">
                <xs:annotation>
                    <xs:documentation>The date that this certificate expires.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
   
    <xs:element name="MiscFormOfPaymentHistory">
        <xs:annotation>
            <xs:documentation>Miscellaneous Form of Payments</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="CreditCardType" use="optional">
                <xs:annotation>
                    <xs:documentation>The 2 letter credit/ debit card type or code which may not have been issued using the standard bank card types  - i.e. an airline issued card</xs:documentation>

                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:length value="2"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="CreditCardNumber" type="common:typeCreditCardNumber" use="optional"/>
            <xs:attribute name="ExpDate" type="xs:gYearMonth" use="optional">
                <xs:annotation>
                    <xs:documentation>The Expiration date of this card in YYYY-MM format.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Text" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>Any free form text which may be associated with the Miscellaneous Form of Payment. This text may be provider or GDS specific</xs:documentation>

                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Category" use="optional">
                <xs:annotation>
                    <xs:documentation>Indicates what Category the Miscellaneous Form Of Payment is being used for payment - The category may vary by GDS.</xs:documentation>

                    <xs:documentation>Allowable values are
                        "Text"
                        "Credit"
                        "CreditCard"
                        "FreeFormCreditCard"
                        "Invoice"
                        "NonRefundable"
                        "MultipleReceivables"
                        "Exchange"
                        "Cash"
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string" />
                </xs:simpleType>
            </xs:attribute>
             <xs:attribute name="AcceptanceOverride" type="xs:boolean" use="optional">
                    <xs:annotation>
                          <xs:documentation>Override airline restriction on the credit card.</xs:documentation>
                   </xs:annotation>
             </xs:attribute>
        </xs:complexType>
    </xs:element>
   
    <xs:element name="AgencyPaymentHistory" type="typeAgencyPaymentHistory">
      <xs:annotation>
         <xs:documentation>Container for Agency Payment</xs:documentation>
      </xs:annotation>
    </xs:element>
   
    <xs:complexType name="typeAgencyPaymentHistory">
        <xs:annotation>
            <xs:documentation>Type for Agency Payment.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="AgencyBillingIdentifier" use="optional">
            <xs:annotation>
                <xs:documentation>Value of the billing id</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                   <xs:maxLength value="128"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="AgencyBillingNumber" use="optional">
            <xs:annotation>
                <xs:documentation>Value of billing number</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                   <xs:maxLength value="128"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="AgencyBillingPassword"  use="optional">
            <xs:annotation>
                <xs:documentation>Value of billing password</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                   <xs:maxLength value="128"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
   </xs:complexType>
  
   <xs:element name="UnitedNationsHistory">
        <xs:annotation>
            <xs:documentation>United Nations Form of Payments</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Number" type="xs:string" use="optional" />
        </xs:complexType>
    </xs:element>
   
    <xs:element name="AgentVoucherHistory">
        <xs:annotation>
            <xs:documentation>Agent Voucher Form of Payments</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Number" type="xs:string" use="optional" />
        </xs:complexType>
    </xs:element>
   
    <xs:complexType name="typeGuaranteeInformationHistory">
                <xs:annotation>
                        <xs:documentation>Information pertaining to the payment of type Guarantee.</xs:documentation>
                </xs:annotation>
                <xs:attribute name="Type" use="optional">
                        <xs:annotation>
                                <xs:documentation>Guarantee only or Deposit</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                                <xs:restriction base="xs:string">
                                        <xs:enumeration value="Guarantee"/>
                                        <xs:enumeration value="Deposit"/>
                                </xs:restriction>
                        </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="AgencyType" use="optional">
                        <xs:annotation>
                                <xs:documentation>Guarantee to Agency IATA or Guarantee to Another Agency IATA</xs:documentation>

                        </xs:annotation>
                        <xs:simpleType>
                                <xs:restriction base="xs:string">
                                        <xs:enumeration value="AgencyIATA"/>
                                        <xs:enumeration value="OtherAgencyIATA"/>
                                </xs:restriction>
                        </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="IATANumber" type="common:StringLength1to128" use="optional">
                        <xs:annotation>
                                <xs:documentation>Payment IATA number. (ie. IATA of Agency or Other Agency)</xs:documentation>

                        </xs:annotation>
                </xs:attribute>
        </xs:complexType>
       
         <xs:complexType name="typeVoucherInformationHistory">
                <xs:annotation>
                        <xs:documentation>Information pertaining to the payment of a Vehicle Rental.</xs:documentation>
                </xs:annotation>
                <xs:attribute name="VoucherType" type="common:typeVoucherType" use="optional">
                        <xs:annotation>
                                <xs:documentation>Specifies if the Voucher is for Full Credit or a Group/Day or a Monetary Amount.</xs:documentation>

                        </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Amount" type="common:typeMoney" use="optional">
                        <xs:annotation>
                                <xs:documentation>Amount associated with the Voucher.</xs:documentation>
                        </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ConfirmationNumber" type="xs:string" use="optional">
                        <xs:annotation>
                                <xs:documentation>Confirmation from the vendor for the voucher</xs:documentation>
                        </xs:annotation>
                </xs:attribute>
                <xs:attribute name="AccountName" type="xs:string" use="optional">
                        <xs:annotation>
                                <xs:documentation>Associated account name for the voucher</xs:documentation>
                        </xs:annotation>
                </xs:attribute>
        </xs:complexType>
        
        <xs:element name="CreditCard">
        <xs:annotation>
            <xs:documentation>UProfile Specific Credit Card Element.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                     <xs:extension base="common:typeCreditCardType">                                
                      <xs:attribute name="ExtractIndicator" type="xs:boolean" use="optional">
                            <xs:annotation>
                                <xs:documentation>This is supported for all Profile Types by Universal Profile.This indicator will be used by MOS to create a Credit Card Extract.</xs:documentation>
                            </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="Active"  type="xs:boolean" use="optional" default="true">
                        <xs:annotation>
                            <xs:documentation>Denotes whether the Credit Card is Active or Not.Default is true</xs:documentation>
                        </xs:annotation>
                 </xs:attribute>             
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
   </xs:element>   
    
    <xs:complexType name="typeAccountTypeProfileInfo">
        <xs:annotation>
            <xs:documentation>History Element for Traveler Group.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="typeProfileInfo">
                <xs:attribute name="MidOfficeID" type="common:StringLength1to255">
                    <xs:annotation>
                        <xs:documentation>Mid Office identifier managed by an external system.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="typeAccountTypeProfileSearchCriteria">
        <xs:annotation>
            <xs:documentation>Search criteria for Accounts, Traveler Groups and Travellers.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="typeProfileSearchCriteria">
                <xs:attribute name="MidOfficeID" type="common:typeWildcard">
                    <xs:annotation>
                        <xs:documentation>Mid Office ID managed by an external system.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="typePolicyReferenceHistory">
        <xs:annotation>
            <xs:documentation>History Element for Accounting Reference</xs:documentation>
        </xs:annotation>
            <xs:complexContent>
                <xs:extension base="typeKeyElement">
                <xs:attribute name="Type" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>A code used to reference a traveler's policy. Valid values are Travelport Universal Policy and Other.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Value" type="common:StringLength1to255">
                    <xs:annotation>
                        <xs:documentation>The number or alphanumeric code for an employer reference.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ControllingProfileID" type="common:typeProfileID">
                    <xs:annotation>
                        <xs:documentation>A list of policies are created in a parent of a traveler, then one of those is applied to the traveler. This profileID is to contain the ID of the Parent who has the list of policies</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="PriorityOrder" type="typePriorityOrder">
                    <xs:annotation>
                        <xs:documentation>Priority order associated with this AccountingReference.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Desc" type="common:StringLength1to255">
                    <xs:annotation>
                        <xs:documentation>An optional description.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:element name="MetaData">
	<xs:annotation>
		<xs:documentation>An element created under the service which will store all settings for the selected profile.</xs:documentation>
	</xs:annotation>
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="MetaDataDetails" minOccurs="1" maxOccurs="99"/>
		</xs:sequence>
		<xs:attribute name="UserType" type="common:StringLength1to50" use="required">
			<xs:annotation>
				<xs:documentation>This attribute will capture which view (say, Agency or Traveler) is called for.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
</xs:element>

<xs:element name="MetaDataDetails">
        <xs:annotation>
            <xs:documentation><xs:annotation>
				<xs:documentation>An element under which all other respective attributes for defining the settings are addressed.</xs:documentation>
			</xs:annotation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="ElementName" type="common:StringLength1to50" use="required">
                <xs:annotation>
                  <xs:documentation>Whatever field/element name is triggered here, this will do the action on that particular field/element in the API database table.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Type" type="common:StringLength1to50" use="optional">
                <xs:annotation>
                    <xs:documentation>Whatever field/element name is triggered here, this will do the action on that particular field/element in the API database table.</xs:documentation>
            </xs:annotation>
            </xs:attribute>  
            <xs:attribute name="Customization" type="common:StringLength1to50" use="required">
                <xs:annotation>
                    <xs:documentation>This attribute captures the option for the action selected by admin in the UI screen, say Hide or View only or Edit.</xs:documentation>
            </xs:annotation>
            </xs:attribute> 
            <xs:attribute name="Key" type="xs:integer">
            	<xs:annotation>
               <xs:documentation>This attribute will capture the unique identifier number assigned to the field.</xs:documentation>
            </xs:annotation>
         </xs:attribute>      
        </xs:complexType>
    </xs:element>

<xs:element name="MetaDataModifyCmd">
      <xs:annotation>
         <xs:documentation>Wrapper for a set of modification commands to be applied to this profile.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:choice>
            <xs:element ref="MetaDataAdd"/>
            <xs:element ref="MetaDataUpdate"/>
            <xs:element ref="MetaDataDelete"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="MetaDataAdd">
      <xs:annotation>
         <xs:documentation>Command to add profile data information</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="MetaData" minOccurs="1" maxOccurs="99"/>        
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="MetaDataUpdate">
      <xs:annotation>
         <xs:documentation>Command to update profile data information</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="MetaData" minOccurs="1" maxOccurs="99"/>          
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="MetaDataDelete">
      <xs:annotation>
         <xs:documentation>Delete  element of given type identified by its key.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:attribute name="Key" type="xs:integer" use="required">
            <xs:annotation>
               <xs:documentation>The identifier of the element that will be deleted from this profile.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   
   <xs:attributeGroup name="attrsUIMetaData">
      <xs:annotation>
         <xs:documentation/>
      </xs:annotation>
    		<xs:attribute name="ProfileID" type="common:typeProfileID" use="required" >
                <xs:annotation>
                   <xs:documentation>This Profile Id would get the ID of the Agency/Account name for which the change is done. Hence all the applicable changes would be done for this particular agency/account.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ProfileType" type="typeProfileType" use="required">
                <xs:annotation>
                  <xs:documentation>This will retrieve the type of the Profile for which the change is triggered, say Agency , Account, etc.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MetaDataVersion" type="common:typeVersion" use="required">
          		<xs:annotation>
             	<xs:documentation>This attribute updates the version of the change or any updates done.</xs:documentation>
          		</xs:annotation>
       		</xs:attribute>
           <xs:attribute name="Description" type="common:StringLength1to128" use="optional">
                <xs:annotation>
                  <xs:documentation>This optional attribute is for any description if the user wants to pass.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
  </xs:attributeGroup>
 </xs:schema>
