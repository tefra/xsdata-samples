                <xs:element ref="TravelDocument" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>The travel document number matched in the search.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="AccountingReference" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>The accounting reference matched in the search.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                 <xs:element ref="PolicyReference" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>The policy reference matched in the search.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="LoyaltyProgramEnrollment" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>The loyalty program matched in the search.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="FormOfPayment" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="FieldData" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>The custom fields matched in the search.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="FieldGroupData" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>The custom fields groups matched in the search.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>            
            <xs:attributeGroup ref="attrsProfileID"/>
            <xs:attribute name="Status" type="common:typeProfileEntityStatus" use="required">
