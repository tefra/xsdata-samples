               </xs:complexType>
            </xs:element>
                <xs:element name="TextRestriction" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Restrictions on profile data for fields with a data type of text (i.e., alpha). Min and max lengths are inclusive. Can define either min/max lengths or enumerations, but not both.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="typeStringRestriction">
                                <xs:attribute name="Action" type="typeAction" use="required">
                                    <xs:annotation>
                                        <xs:documentation>Indicate the action to be executed (add, update or delete)</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="AlphaNumericRestriction" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Restrictions on profile data for fields with a data type of alphanumeric. Min and max lengths are inclusive. Can define either min/max lengths or enumerations, but not both.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="typeStringRestriction">
                                <xs:attribute name="Action" type="typeAction" use="required">
                                    <xs:annotation>
                                        <xs:documentation>Indicate the action to be executed (add, update or delete)</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="PercentageRestriction" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Restrictions on profile data for fields with a data type of percentage. Min and max values are inclusive.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="typeFloatRestriction">
                                <xs:attribute name="Action" type="typeAction" use="required">
                                    <xs:annotation>
                                        <xs:documentation>Indicate the action to be executed (add, update or delete)</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
         </xs:sequence>
         <xs:attribute name="ID" type="typeFieldID" use="optional">
            <xs:annotation>
               <xs:documentation>Unique identifier of the field.Id is required for update or delete action.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attributeGroup ref="attrsField"/>
         <xs:attribute name="Action" type="typeAction" use="required">
            <xs:annotation>
               <xs:documentation>Indicate the action to be executed (add, delete, etc.)</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="Force" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>To specify whether this is a Force Update or Force Delete.</xs:documentation>
            </xs:annotation>
