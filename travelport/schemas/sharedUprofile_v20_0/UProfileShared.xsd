    <xs:element name="TravelerInfo">
        <xs:annotation>
            <xs:documentation>Traveler specific profile information</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeAccountTypeProfileInfo">
                    <xs:sequence>
                        <xs:element ref="Address" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Traveler Address</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Traveler Phone Number</xs:documentation>
                            </xs:annotation>
                        </xs:element>
