	<xs:import namespace="http://www.travelport.com/schema/common_v52_0" schemaLocation="../common_v52_0/CommonReqRsp.xsd"/>
	<xs:import namespace="http://www.travelport.com/schema/air_v52_0" schemaLocation="../air_v52_0/Air.xsd"/>
	<xs:import namespace="http://www.travelport.com/schema/vehicle_v52_0" schemaLocation="../vehicle_v52_0/Vehicle.xsd"/>
	<xs:import namespace="http://www.travelport.com/schema/hotel_v52_0" schemaLocation="../hotel_v52_0/Hotel.xsd"/>
	<xs:import namespace="http://www.travelport.com/schema/rail_v52_0" schemaLocation="../rail_v52_0/Rail.xsd"/>
