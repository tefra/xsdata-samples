							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="RetainReservation" type="typeRetainReservation" use="optional" default="None">
						<xs:annotation>
							<xs:documentation>Provider: 1G,1V,1P</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Source" use="optional">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="50"/>
