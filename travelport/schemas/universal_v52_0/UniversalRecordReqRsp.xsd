<xs:schema xmlns="http://www.travelport.com/schema/universal_v52_0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="http://www.travelport.com/schema/common_v52_0" xmlns:air_v52_0="http://www.travelport.com/schema/air_v52_0" xmlns:vehicle="http://www.travelport.com/schema/vehicle_v52_0" xmlns:hotel="http://www.travelport.com/schema/hotel_v52_0" xmlns:rail="http://www.travelport.com/schema/rail_v52_0" xmlns:passive="http://www.travelport.com/schema/passive_v52_0" targetNamespace="http://www.travelport.com/schema/universal_v52_0" elementFormDefault="qualified" attributeFormDefault="unqualified">
