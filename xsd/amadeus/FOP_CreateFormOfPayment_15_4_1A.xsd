  <xs:complexType name="AuthenticationDataType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Conveys CC authentication data</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="veres" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">VERes status (enrollment) Values : Y : authentication available N : cardholder not participating U : Unable to authenticate E : error message</xs:documentation>
