                  <xs:documentation xml:lang="en">This group conveys data related to the FOP.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="tax" minOccurs="0" maxOccurs="3" type="TaxTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Taxes information</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="fareCalcDetails" minOccurs="0" type="CodedAttributeType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the fare calculation details in all cases.   In specific cases (market/airline specifics) it conveys also: - the revenue attribution number/pseudo IATA number (entered in FZRA) - the payment on demand number generated at issuance time</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="pricingIndicator" minOccurs="0" type="PricingTicketingDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the pricing indicator.</xs:documentation>
                </xs:annotation>
              </xs:element>
