                <xs:annotation>
                  <xs:documentation xml:lang="en">Conveys the information about the tour code associated to the document.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="invoiceDetails" minOccurs="0" type="DocumentInformationDetailsTypeI">
                <xs:annotation>
