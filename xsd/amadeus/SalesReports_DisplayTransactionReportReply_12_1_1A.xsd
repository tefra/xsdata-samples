                      </xs:annotation>
                    </xs:element>
                    <xs:element name="couponAssociated" minOccurs="0" type="CouponInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Coupon numbers and SAC number</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
