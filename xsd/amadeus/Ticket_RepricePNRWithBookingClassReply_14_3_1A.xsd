<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/TARIPR_14_3_1A" xmlns="http://xml.amadeus.com/TARIPR_14_3_1A" elementFormDefault="qualified">
  <xs:element name="Ticket_RepricePNRWithBookingClassReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">Ticket Document Voluntary Update Reply
To return a list of fares for existing PNR itinerary booking class
TARIPR is the response of repricing the new itinerary in the current booking class.  Only one fare is returned in that case.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="errorMessage" minOccurs="0" type="ErrorGroupType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Error message at Tickets and document level</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ticketGroup" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">Ticket exchange group</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="pnrLocatorData" type="ReservationControlInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">PNR record locator information for this transaction. This PNR record locator is used for tracing purpose.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="errorMessage" minOccurs="0" type="ErrorGroupType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Error message at Tickets and document level</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="fareList" minOccurs="0" maxOccurs="99">
                <xs:annotation>
                  <xs:documentation xml:lang="en">List of fares returned by Fare Quote server for the current PNR</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="pricingInformation" type="PricingTicketingSubsequentTypeI_163004S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Pricing information such as pricing rule and sales indicator.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="fareReference" type="ItemReferencesAndVersionsType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Fare reference number. Ordering information is not relevant here.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="lastTktDate" minOccurs="0" type="StructuredDateTimeInformationType_163006S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Last date to ticket the fare.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="validatingCarrier" minOccurs="0" type="TransportIdentifierType_163007S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Validating carrier identification.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="paxSegReference" type="ReferenceInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Passenger/segment association of the fare is specified here.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="fareDataInformation" type="MonetaryInformationType_163002S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Specify all fare data information (base fare, equivalent fare, new ticket cheaper, Mileage Accrual...)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="taxInformation" minOccurs="0" maxOccurs="120">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="taxDetails" type="DutyTaxFeeDetailsTypeU">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Tax details</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="amountDetails" minOccurs="0" type="MonetaryInformationTypeI_163018S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Amount details. If the tax is a passenger facility charge (PFC) the detail of the airports related taxes is given here.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="bankerRates" minOccurs="0" type="ConversionRateTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Banker's rates are used to convert amounts of the TST (converts base fare to equivalent fare) 1st C661 : 1st bankers' rate which is a percentage (no currency) 2nd C661 : 2nd bankers' rate which is currency+amount.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="passengerInformation" minOccurs="0" maxOccurs="99">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="penDisInformation" type="DiscountAndPenaltyInformationTypeI_163020S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Penalty/discount details specified in the request.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="passengerReference" minOccurs="0" type="ReferenceInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Reference of passengers that have a type code.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="originDestination" minOccurs="0" type="OriginAndDestinationDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Origin and destination of the fare. 1st C3225 occurence : origin city. 2nd C3225 occurence : destination city</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="segmentInformation" minOccurs="0" maxOccurs="96">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="connexInformation" type="ConnectionTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Connection information.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="segDetails" minOccurs="0" type="TravelProductInformationTypeI_163025S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Details on open segments added to the price calculation. These open segments exist only in the fare calculated, they have no equivalent in the PNR itinerary. This segment gives also information on booking class for best buy transactions.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="fareQualifier" minOccurs="0" type="FareQualifierDetailsTypeI_163023S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Fare basis information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="validityInformation" minOccurs="0" maxOccurs="2" type="StructuredDateTimeInformationType_163006S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Validity information for this fare</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="bagAllowanceInformation" minOccurs="0" type="ExcessBaggageTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Baggage allowance information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="segmentReference" minOccurs="0" type="ReferenceInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Reference of the segment associated to the group.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="sequenceInformation" minOccurs="0" type="ItemReferencesAndVersionsType_163024S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">The segment order in the pricing response can be different than the one of the PNR itinerary (segments are reordered at price calculation time). This order inform,ation is conveyed by the sequence number. If this order information is not present then the order is by default the one of the PNR.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="otherPricingInfo" minOccurs="0" maxOccurs="5" type="CodedAttributeType_162999S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Other pricing information such as endorsement, tour name...</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="warningInformation" minOccurs="0" maxOccurs="99">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="warningCode" type="ApplicationErrorInformationType_163026S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Fare warning information code.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="warningText" minOccurs="0" type="InteractiveFreeTextTypeI_163027S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Description in free flow text of the warning concerning the fare.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="automaticReissueInfo" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Automatic Reissue Informations</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="ticketInfo" type="TicketNumberTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment contains the original ticket number.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="couponInfo" type="CouponInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment contains the coupon number (in absolute) corresponding to the first coupon for use from the last flawn segment.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="paperCouponRange" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">provide the range for paper reissuance</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="ticketInfo" type="TicketNumberTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">This segment contains the original ticket number.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="couponInfo" type="CouponInformationTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">This segment contains the coupon number (in absolute) corresponding to the first coupon for use from the last flawn segment.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="baseFareInfo" type="MonetaryInformationTypeI_163010S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Base fare Information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="firstDpiGroup">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="reIssuePenalty" type="DiscountAndPenaltyInformationTypeI_163012S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Penalty amount in reissue currency</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="reissueInfo" type="MonetaryInformationType_163013S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Reissue Informations</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="oldTaxInfo" type="MonetaryInformationType_163013S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Old Tax informations</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="reissueBalanceInfo" type="MonetaryInformationType_163015S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Balance Reissue Informations</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="secondDpiGroup">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="penalty" type="DiscountAndPenaltyInformationTypeI_163012S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Discount and penalty info.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="residualValueInfo" type="MonetaryInformationType_163013S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Residual Value information</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="oldTaxInfo" type="MonetaryInformationType_163013S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Old Tax informations</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="issueBalanceInfo" type="MonetaryInformationType_163015S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Balance issue Informations</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="thirdGroup" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="reissueMilesInfo" type="MonetaryInformationType_163016S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Reissue Miles Information</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="originalMilesValues" minOccurs="0" type="MonetaryInformationType_163016S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Original Miles Values</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="reissueAttributes" minOccurs="0" type="CodedAttributeType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">this segment conveys specific reissue attributes like Revalidation flag.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="fareComponentDetailsGroup" minOccurs="0" maxOccurs="99" type="FareComponentDtlsType" />
                    <xs:element name="markerFares" type="DummySegmentTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Marker  fares</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="docSeparator" type="DummySegmentTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">docSeparator</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="groupEmd" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="technicalStatusInfo" type="StatusType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Technical status information</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="nbOfPassengersInformation" minOccurs="0" type="NumberOfUnitsType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Number of passengers information</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="travellerInfo" minOccurs="0" maxOccurs="999" type="TravellerInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Traveller information</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="errorInfoGroup" minOccurs="0" type="ErrorGroupType" />
              <xs:element name="mnrDescriptionInfoGrp" minOccurs="0" maxOccurs="999">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Minirule description information group</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="mnrPricedInfo" type="SegmentRepetitionControlType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Pricing solution information on which minirules applies</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="mnrRulesInfoGrp" minOccurs="0" maxOccurs="600" type="MiniRulesRegulPropertiesType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Rules information to be displayed for a current pricing solution</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="markerFares" type="DummySegmentTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Marker  fares</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="monetaryInfoGroup" minOccurs="0" maxOccurs="150">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="amountType" type="StatusType_162980S" />
                    <xs:element name="currencyInfos" minOccurs="0" type="ConversionRateType_162872S" />
                    <xs:element name="monetaryInfo" minOccurs="0" type="MonetaryInformationType_162993S" />
                    <xs:element name="taxDetailsInfoGroup" minOccurs="0" maxOccurs="99">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Tax details information group</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="taxDetailsListInfo" type="TaxType_162997S" />
                          <xs:element name="discountAndPenaltynfo" minOccurs="0" type="DiscountAndPenaltyInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">VAT, penalties for the elementary tax</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="elementaryTaxSubDetails" minOccurs="0" maxOccurs="5" type="MonetaryInformationType_162875S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Tax sub details for ZP and PFC</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="markerAllFares" type="DummySegmentTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Marker all fares</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="allFaresInfoGroup" minOccurs="0" maxOccurs="999">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="markerAllFares" type="DummySegmentTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Marker all fares</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="responseTypeInfo" minOccurs="0" type="StatusType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Type of the response</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="responseIdentInfo" minOccurs="0" type="ItemNumberType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Response identification information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="interactiveFreeText" minOccurs="0" maxOccurs="99" type="InteractiveFreeTextType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Interactive free text</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="involvedCurrenciesInfo" minOccurs="0" type="ConversionRateType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Currency of travel origin, currency of sale</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="monetaryInfo" minOccurs="0" type="MonetaryInformationType_162861S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Monetary information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="penaltyInfo" minOccurs="0" type="DiscountAndPenaltyInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Penalty amounts</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="computedTaxInfoGroup" minOccurs="0" maxOccurs="5">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Computed tax information group at itinerary level</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="taxesInfos" type="TaxType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Details of taxes</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="computedDiscountAndPenaltyInfo" minOccurs="0" type="DiscountAndPenaltyInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">VAT, penalties for the elementary tax</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="computedTaxSubDetails" minOccurs="0" maxOccurs="5" type="MonetaryInformationType_162875S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Tax sub details for ZP and PFC</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="monetaryInfoGroup" minOccurs="0" maxOccurs="20" type="MonetaryInformationGroupType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Monetary and Taxes information group</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="emdInfoGroup" minOccurs="0" maxOccurs="999">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Emd and coupon information group</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="passengerRef" type="SpecificTravellerType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">passenger reference</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="dateInfos" minOccurs="0" type="StructuredDateTimeInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Issuance date Infos</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="refInfo" minOccurs="0" type="AttributeType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">emd reference info</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="freetext" minOccurs="0" type="FreeTextInformationType_162882S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Reject message</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="emdCoupon" minOccurs="0" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Emd coupon information group</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="refInfo" type="ReferenceInfoType_162883S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">coupon and passenger references</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="attributeInfo" minOccurs="0" type="AttributeType">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">coupons reference</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="freetext" minOccurs="0" type="FreeTextInformationType_162882S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Commercial name info</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="segmentRange" minOccurs="0" type="RangeDetailsType">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Old segment range</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="monetaryInfoGroup" minOccurs="0" maxOccurs="20" type="MonetaryInformationGroupType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">monetary and taxes information group</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="ticketingInfoGroup" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Ticketing information group</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="pricingTicketingDetails" type="PricingTicketingDetailsType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Sale and ticketing information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="fareQualifierDetails" minOccurs="0" type="FareQualifierDetailsTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Fare qualifier details</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="originalOriDesInfo" minOccurs="0" type="OriginAndDestinationDetailsType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Origin and destination in the original issued ticket</xs:documentation>
