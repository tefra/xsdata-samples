  <xs:complexType name="SailingCategoryInfoType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Provides category information for the specified sailing.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="SailingInfoType">
        <xs:sequence>
