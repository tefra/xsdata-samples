          <xs:documentation xml:lang="en">Defines the particular type of hotel (e.g., golf, ski, bed and breakfast).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:attribute name="Code" type="OTA_CodeType" use="optional">
