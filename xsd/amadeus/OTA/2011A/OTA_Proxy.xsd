                        <xs:documentation xml:lang="en">Information about the supplier.</xs:documentation>
                      </xs:annotation>
                    </xs:attributeGroup>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
              <xs:attribute name="TransferAction" type="TransferActionType" use="optional">
