                  </xs:annotation>
                </xs:attribute>
                <!--Replaced incorrect type "xs:positiveInteger"-->
                <xs:attribute name="MaxOccupancy" type="xs:long" use="optional">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">Maximum number of guests allowed in a room type.</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
                <!--Replaced incorrect type "xs:nonNegativeInteger"-->
                <xs:attribute name="MaxAdultOccupancy" type="xs:long" use="optional">
                  <xs:annotation>
