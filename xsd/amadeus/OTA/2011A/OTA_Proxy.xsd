                                  <xs:documentation xml:lang="en">Multimedia information about the amenity.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="DescriptiveText" type="xs:string" minOccurs="0">
                                <xs:annotation>
                                  <xs:documentation xml:lang="en">Descriptive text that describes the amenity.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                            </xs:sequence>
                            <xs:attribute name="RoomAmenityCode" type="OTA_CodeType" use="optional">
                              <xs:annotation>
