                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="Type" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation xml:lang="en">The type of the rate policy e.g. government and preferred.
Select a predefined value from the list or use the "Other_" value to create an enumerated value agreed upon between trading partners.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType>
                            <xs:simpleContent>
