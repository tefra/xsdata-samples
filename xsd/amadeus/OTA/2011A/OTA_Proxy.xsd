                      <xs:annotation>
                        <xs:documentation xml:lang="en">Refund was calculated manually.</xs:documentation>
                      </xs:annotation>
                    </xs:enumeration>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:complexType>
