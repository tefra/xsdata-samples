      <xs:element name="PetInfo" type="StringLength1to64" minOccurs="0" maxOccurs="3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Describes the customer's pet.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaymentForm" minOccurs="0" maxOccurs="100">
