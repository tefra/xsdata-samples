              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <!--Replaced incorrect type "xs:nonNegativeInteger"-->
          <xs:attribute name="MinOccupancy" type="xs:long" use="optional">
            <xs:annotation>
              <xs:documentation xml:lang="en">The minimum number of people in this meeting room.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <!--Replaced incorrect type "xs:nonNegativeInteger"-->
          <xs:attribute name="MaxOccupancy" type="xs:long" use="optional">
            <xs:annotation>
              <xs:documentation xml:lang="en">The maximum number of people in this meeting room.</xs:documentation>
