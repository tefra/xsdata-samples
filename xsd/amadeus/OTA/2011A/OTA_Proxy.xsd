            </xs:annotation>
          </xs:attribute>
          <!--Replaced incorrect type "xs:nonNegativeInteger"-->
          <xs:attribute name="RoomNightTargetQuantity" type="xs:long" use="optional">
            <xs:annotation>
              <xs:documentation xml:lang="en">Defines minimum room nights.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <!--Replaced incorrect type "xs:nonNegativeInteger"-->
          <xs:attribute name="RoomNightProductionQuantity" type="xs:long" use="optional">
            <xs:annotation>
              <xs:documentation xml:lang="en">Allows you to pass the number of room nights previously produced.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attributeGroup ref="DateTimeSpanGroup">
            <xs:annotation>
              <xs:documentation xml:lang="en">These are the dates to which this proposal applies.</xs:documentation>
