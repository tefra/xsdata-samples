          <xs:documentation>Type of rerouting management action instigated by operator.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="roadMaintenanceType" type="sit:_RoadMaintenanceTypeEnum" minOccurs="0" maxOccurs="unbounded">
