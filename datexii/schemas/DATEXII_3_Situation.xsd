        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="technicalProblems">
        <xs:annotation>
          <xs:documentation>Technical problems.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="vandalism">
