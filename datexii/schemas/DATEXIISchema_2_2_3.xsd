      </xs:element>
      <xs:element name="vmsDynamicCharacteristics" type="D2LogicalModel:VmsDynamicCharacteristics" minOccurs="0" />
      <xs:element name="vmsFault" type="D2LogicalModel:VmsFault" minOccurs="0" maxOccurs="unbounded" />
