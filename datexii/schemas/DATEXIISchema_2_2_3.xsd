        </xs:annotation>
      </xs:element>
      <xs:element name="delays" type="D2LogicalModel:Delays" minOccurs="0" />
      <xs:element name="impactExtension" type="D2LogicalModel:_ExtensionType" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
